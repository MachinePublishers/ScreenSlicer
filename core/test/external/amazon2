browseNodeMap.n1064954 = "officeBN";
browseNodeMap.n468642 = "videogamesBN";
browseNodeMap.n1036592 = "clothingBN";
browseNodeMap.n4991425011 = "collectiblesBN";
browseNodeMap.n3760911 = "beautyBN";
browseNodeMap.n5174 = "musicBN";
