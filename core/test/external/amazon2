amznJQ.available("jQuery", function() {
    jQuery(window).load(function() {

        amznJQ.available("search-js-general" , function() {
                window.precacheDetailImages = function(imageUrls, pids) {

                    function transformUrl(imgUrl, pid) {
                        if(typeof imgUrl !== 'string' || typeof imgUrl === 'undefined' || !imgUrl) {
                            return;
                        }
                        
                        var suffix               = '._SL500_AA300_.jpg',
                            defaultApparel       = '._AA300_.jpg',
                            iVApparel            = '._SX300_SY390_CR,0,0,300,390_.jpg',
                            imgUrlSplit          = imgUrl.split("._");

                        if(imgUrlSplit.length) {
                            var prefix = imgUrlSplit[0];
                            if((!pid  &amp;&amp; storeName == "books") || pid == "books_display_on_website") {
                                if(imgUrl.match("PIsitb-sticker-arrow-dp")){
                                    var OUID = imgUrl.substr(imgUrl.indexOf('_OU'), 6);                                     
                                    var lookInsideSticker    = '._BO2,204,203,200_PIsitb-sticker-arrow-click,TopRight,35,-76_AA300_SH20'+ OUID +'.jpg';
                                    urls.push(prefix + lookInsideSticker);
                                } else {
                                    urls.push(prefix + suffix);
                                }
                            } else if((!pid &amp;&amp; storeName == "apparel") || pid == "apparel_display_on_website") {
                                    urls.push(prefix + "._SX342_.jpg");
                                    urls.push(prefix + "._SY445_.jpg");
                            } else if((!pid &amp;&amp; storeName == "shoes") || pid == "shoes_display_on_website") {
                                    urls.push(prefix + "._SX395_.jpg");
                                    urls.push(prefix + "._SY395_.jpg");
                            } else {
                                urls.push(prefix + suffix);
                            }
                        }
                    };

                    var dpImages = [],
                        urls     = [],
                        numImgsPreload = Math.min(4, imageUrls.length),
                        storeName = "gateway";

                    for(var i = 0; i &lt; numImgsPreload; i++){
                        var currPid = (pids &amp;&amp; pids.length) ? pids[i] : "";
                        transformUrl(imageUrls[i], currPid);
                    }

                    for(var j = 0; j &lt; urls.length; j++) {
                        var img = new Image();
                        img.src = urls[j];
                        dpImages.push(img);
                    }

                    window.dpImages = dpImages;

                };// precache function ends
        });
    });
});


//]]>
        </script>
        <script type="text/javascript">
//<![CDATA[

  if (window.ue) { // Check if page is instrumented
    !(function(){
      try {
        // This should not impact to the latency
        if(typeof(amzn) !== 'undefined' &amp;&amp; amzn &amp;&amp; amzn.sx &amp;&amp; amzn.sx.utils &amp;&amp; amzn.sx.utils.jsDepMgr) {
          amzn.sx.utils.jsDepMgr.when('jQuery search-js-general', 'prerenderLink',  function ($) {
            // declare a function which registers event handler to ATF div
            function attachToAtf(){
              function prerenderLink(url, rel, treatment) {
                // Add scope to show that we have decided to pre-render for the
                // scope passed as argument
                uex("ld", control_scope + "_" + treatment + "_PRE", {wb:1});

                // Determine if we pre-render correctly. This is done by
                // checking if the link we prerendered was clicked
                var prerendered_link = $("'a[href=\"" + url + "\"]'");
                prerendered_link.click(function() {
                  uex("ld", control_scope + "_" + treatment + "_CORRECT", {wb:1});
                });

              }; //prerenderLink

              var pre_rendered = false;
              var control_scope = "PE_PRERENDER";
              var results = SearchPageAccess.asinDpLinks();
              var length = results ? results.length : 0;

              // T4 algorithm - Prerender on mouse up over ASIN div
                uex("ld", control_scope + "_T4", {wb:1});
                for(var i = 0; i &lt; length; i++) {
                  $("'a[href=\"" + results[i].href + "\"]'").each(function() {
                    var $this = $(this);
                    $this.mouseup(function() {
                      if(!pre_rendered) {
                        prerenderLink($this.attr("href"), 'prerender', "T4");
                        pre_rendered = true;
                      }
                    });
                  });
                }
            } //attachToATF

            // hook it up with ATF event for ajax
            SPUtils.afterEvent("spATFEvent", attachToAtf);
          }); //search general
        }
      } catch(exception) {
        if (window.ueLogError) {
          var additionalInfo = {
            logLevel : 'ERROR',
            attribution : 'precache-dp-assets',
            message : 'Pre-render DP failed'
          };
          window.ueLogError(exception, additionalInfo);
        }
      }
    })();
  }

//]]>
        </script>
        <script type="text/javascript">
//<![CDATA[

if (window.amznJQ) {
  amznJQ.available('search-js-general', function() {
    jQuery(window).load(function () {
    var p = window.precacheDetailImages,
        s = window.SearchPageAccess,
        $ = jQuery;
    if (p &amp;&amp; s) {
      var u = function() {
        var idivs = s.asinImages(),
            pg/* = s.asinProductGroups()*/,
            img = [], i = idivs.length;
        while (i-- &gt; 0) {
          img[i] = $(idivs[i]).find('&gt; a &gt; img').attr('src');
        }
        p(img, pg);
      };
      u();
      amznJQ.available('search-ajax', function() {
        $(document).bind('searchajax', u);
      });
    }
  });
 });
}

