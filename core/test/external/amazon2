    <script type="text/javascript">
//<![CDATA[

            !function() {
            	var hash = location.href.match(/#.*/);
            	if (!hash) {
            		location.href = '/';
            	} else {
            		hash = hash[0].substr(1);
            		if (hash.charAt(0) != '/') {
            			location.href = '/';
            		} else {
            			setTimeout(function() {
                			if (!document.getElementById('searchTemplate')) {
                				location.href = '/s' + hash + '&amp;ajr=sabc';
                			}
                		}, 5000);
            		}
            	}
            	
            	amznJQ.available('search-ajax', function() {
            		if (!window.jQuery || !window.jQuery.searchAjax) {
            			location.href = '/s' + hash + '&amp;ajr=sabc';
            		}
            	});
            }();
        
//]]>
    </script>
