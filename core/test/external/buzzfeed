	&lt;p class=&quot;num-results&quot;&gt;Latest hot buzz for &lt;span&gt;${search_terms|html}&lt;/span&gt;:&lt;/p&gt;

{if times}
	&lt;p&gt;&lt;strong&gt;total time:&lt;/strong&gt;      ${times.total_time}&lt;/p&gt;
	&lt;p&gt;&lt;strong&gt;partner search:&lt;/strong&gt;  ${times.partner}&lt;/p&gt;
	&lt;p&gt;&lt;strong&gt;buzzfeed search:&lt;/strong&gt; ${times.buzzfeed}&lt;/p&gt;
	&lt;p&gt;-----------------------&lt;/p&gt;
	&lt;p&gt;partner sphinx search: ${times.partner_sphinx_search}&lt;/p&gt;
	&lt;p&gt;was triggered: ${times.was_triggered}&lt;/p&gt;
{/if}
&lt;ul class=&quot;flow&quot;&gt;
{for buzz in results}
	{if buzz.type == &apos;buzzfeed&apos;}

     &lt;li class=&quot;post2 buzz border c&quot; {if buzz.user_ad}style=&quot;background-color: #FDF6E5;&quot;{/if}&gt;
      &lt;div class=&quot;thumb&quot;&gt;
       {if buzz.form == &apos;link&apos; &amp;&amp; buzz.link_buzz}
        &lt;a href=&quot;${buzz.link_buzz}&quot;&gt;
       {else}
        &lt;a href=&quot;${buzz.campaign_uri}&quot;&gt;
       {/if}
        &lt;img src=&quot;http://s3-ak.buzzfeed.com${buzz.image}&quot; alt=&quot;&quot; title=&quot;${buzz.name}&quot; height=&quot;83&quot; width=&quot;125&quot; /&gt;
       &lt;/a&gt;

      &lt;/div&gt; &lt;!-- .thumb --&gt;

      &lt;div class=&quot;info&quot;&gt;
       {if buzz.form == &apos;link&apos; &amp;&amp; buzz.link_buzz}
        &lt;h3&gt;&lt;a href=&quot;${buzz.link_buzz}&quot; class=&quot;trend&quot;&gt;${buzz.name} &lt;img src=&quot;http://s3-ak.buzzfed.com/static/images/public/icons/external-link2.png&quot; width=&quot;10&quot; height=&quot;10&quot; alt=&quot;[link]&quot; /&gt;&lt;/a&gt;&lt;/a&gt;&lt;/h3&gt;
       {else}
        &lt;h3&gt;&lt;a href=&quot;${buzz.campaign_uri}&quot; class=&quot;trend&quot;&gt;${buzz.name}&lt;/a&gt;&lt;/h3&gt;
       {/if}
       &lt;p class=&quot;description&quot;&gt;
        &lt;span{if buzz.short_description &amp;&amp; !buzz.commentary_raw_html}class=&quot;blurb&quot;{/if}&gt;${buzz.blurb}&lt;/span&gt; 
       &lt;/p&gt;

       &lt;div class=&quot;meta&quot;&gt;          
        &lt;div class=&quot;user-info&quot;&gt;
         &lt;a href=&quot;/${buzz.username}&quot;&gt;
          {if buzz.user_image}&lt;img src=&quot;http://s3-ak.buzzfeed.com${buzz.user_image}&quot; width=&quot;25&quot; height=&quot;25&quot; alt=&quot;&quot; /&gt;
          {else}&lt;img src=&quot;http://s3-ak.buzzfeed.com/static/images/public/defaults/user.jpg&quot; width=&quot;25&quot; height=&quot;25&quot; alt=&quot;&quot; /&gt;
          {/if}
         &lt;/a&gt;
         &lt;div class=&quot;user-info-info&quot;&gt;
          &lt;a class=&quot;user-name&quot; href=&quot;/${buzz.username}&quot;&gt;${buzz.user}&lt;/a&gt;&lt;br /&gt;
          &lt;span class=&quot;ago&quot;&gt;posted on ${buzz.published_unix|strftime:&apos;%b %d, %Y&apos;}&lt;/span&gt;
          {if buzz.form == &apos;link&apos; &amp;&amp; buzz.link_buzz}
            ·  &lt;a class=&quot;comment_icon&quot; href=&quot;${buzz.campaign_uri}&quot;&gt; &lt;/a&gt;
          {/if}
         &lt;/div&gt;
        &lt;/div&gt; &lt;!-- .user-info --&gt;

        {if buzz.debug}
          &lt;ul&gt;
           &lt;li&gt;total viral views: &lt;strong&gt;${buzz.viral_views|add_commas}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;&lt;strong&gt;sorting score:&lt;/strong&gt; ${buzz.viral_score.score}&lt;/li&gt;
           &lt;li&gt;&lt;strong&gt;formula:&lt;/strong&gt; VV + VL + REL - T&lt;/li&gt;
           &lt;li&gt;viral views score (VV): &lt;strong&gt;${buzz.viral_score.viral_views}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;viral lift score (VL): &lt;strong&gt;${buzz.viral_score.viral_lift}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;relevance score (REL): &lt;strong&gt;${buzz.viral_score.relevance}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;time score (T): &lt;strong&gt;${buzz.viral_score.time}&lt;/strong&gt;&lt;/li&gt;
         &lt;ul&gt;
        {/if}

       &lt;/div&gt; &lt;!-- .meta --&gt;
       {if buzz.user_ad}
		&lt;div class=&quot;search-ad-featured-partner&quot; style=&quot;width:485px;font-size:9px;color:#999;text-align:right;&quot;&gt;
			&lt;span style=&quot;margin-right:10px;margin-bottom:10px;&quot;&gt;FEATURED PARTNER&lt;/span&gt;
		&lt;/div&gt;
       {/if}  
      &lt;/div&gt; &lt;!-- .info --&gt;
     &lt;/li&gt;

	{else}

     &lt;li class=&quot;post2 border border_partner c&quot;&gt;
      &lt;div class=&quot;favicon&quot;&gt;&lt;a href=&quot;${buzz.buzz_url}&quot;&gt;&lt;img src=&quot;http://www.google.com/s2/favicons?domain=${buzz.domain}&quot; height=&quot;16&quot; width=&quot;16&quot; title=&quot;${buzz.display_name}&quot;/&gt;&lt;/a&gt;&lt;/div&gt;
      &lt;div class=&quot;info_partner&quot;&gt;
       &lt;h3&gt;&lt;a href=&quot;${buzz.buzz_url}&quot; class=&quot;trend title_partner&quot;&gt;${buzz.name} &lt;img src=&quot;http://s3-ak.buzzfed.com/static/images/public/icons/external-link2.png&quot; width=&quot;10&quot; height=&quot;10&quot; alt=&quot;[link]&quot; /&gt;&lt;/a&gt;&lt;/h3&gt;
       &lt;div class=&quot;meta_partner&quot;&gt;
        &lt;div class=&quot;user-info-partner&quot;&gt;
         &lt;div class=&quot;user-info-info-partner&quot;&gt;
          &lt;span class=&quot;ago_partner&quot;&gt;posted on &lt;a class=&quot;user-name&quot; href=&quot;/${buzz.username}&quot;&gt;${buzz.display_name}&lt;/a&gt; on ${buzz.first_seen|strftime:&apos;%b %d, %Y&apos;}&lt;/span&gt;
         &lt;/div&gt;
         
        &lt;/div&gt;

        {if buzz.debug}
          &lt;p&gt;---------&lt;/p&gt;
          &lt;p&gt;&lt;strong&gt;queries:&lt;/strong&gt; ${buzz.queries}&lt;/p&gt;
          &lt;p&gt;---------&lt;/p&gt;
          &lt;ul&gt;
           &lt;li&gt;total viral views: &lt;strong&gt;${buzz.viral_views|add_commas}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;&lt;strong&gt;sorting score:&lt;/strong&gt; ${buzz.viral_score.score}&lt;/li&gt;
           &lt;li&gt;&lt;strong&gt;formula:&lt;/strong&gt; VV + VL + REL - T&lt;/li&gt;
           &lt;li&gt;viral views score (VV): &lt;strong&gt;${buzz.viral_score.viral_views}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;viral lift score (VL): &lt;strong&gt;${buzz.viral_score.viral_lift}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;relevance score (REL): &lt;strong&gt;${buzz.viral_score.relevance}&lt;/strong&gt;&lt;/li&gt;
           &lt;li&gt;time score (T): &lt;strong&gt;${buzz.viral_score.time}&lt;/strong&gt;&lt;/li&gt;
         &lt;ul&gt;
        {/if}

       &lt;/div&gt;
      &lt;/div&gt;
     &lt;/li&gt;


	{/if}

{/for}

&lt;/ul&gt;

</textarea>        <script type="text/javascript">
