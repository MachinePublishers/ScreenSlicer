    <!-- support/ads/dart_init.ett :end -->    <script type="text/javascript">
//<![CDATA[

mi__dart_demo_gender = dart_ad_cookie_gender;
mi__dart_demo_birth_year = dart_ad_cookie_birth_year;
mi__dart_demo_income = dart_ad_cookie_income;

if (mi__dart_demo_gender || mi__dart_demo_birth_year || mi__dart_demo_income) {
  mi__ad_cookie = 1;
}

if (mi__dart_demo_birth_year) {
  today_year = new Date().getFullYear();
  age = today_year - mi__dart_demo_birth_year;
  if (age &lt; 13) {}
  else if (age &gt;= 13 &amp;&amp; age &lt;=17) {
    mi__dart_demo_age = '13-17';
  }
  else if (age &lt;= 20) {
    mi__dart_demo_age = '18-20';
  }
  else if (age &gt;= 76) {
    mi__dart_demo_age = '76-200';
  }
  else {
    if (age % 5 == 0) {
      min_age = age - 4;
      max_age = age;
    }
    else {
      min_age = age - (age % 5) + 1;
      max_age = age + (5 - (age % 5));
    }
    mi__dart_demo_age = min_age + "-" + max_age;
  }
}

// Since this is first pass TT only, ord must be done outside of TT, sooo...js.
  var mi__gpt_ord = Math.floor(Math.random()*1000000000000000);

// Standard Google Tag init stuff
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];

  (function() {
    var gads = document.createElement("script");
    gads.async = true;
    gads.type = "text/javascript";
    var useSSL = "https:" == document.location.protocol;
    gads.src = (useSSL ? "https:" : "http:") + "//www.googletagservices.com/tag/js/gpt.js";
    var node =document.getElementsByTagName("script")[0];
    node.parentNode.insertBefore(gads, node);
  })();

// End Standard Google Tag init stuff

// make this safer for sending
  var gpt_ref = document.referrer;
  gpt_ref = gpt_ref.replace("http://","");
  gpt_ref = gpt_ref.split("/")[0];
  gpt_ref = gpt_ref.split("#")[0];
  gpt_ref = gpt_ref.split("?")[0];

// Start defining the ads
  googletag.cmd.push(function() {
    googletag.pubads().setTargeting("pl", "search");
    googletag.pubads().setTargeting("sect", "Search");

    googletag.pubads().setTargeting("ref", gpt_ref);
    if (mi__ad_cookie) {
      googletag.pubads().setTargeting("gender", mi__dart_demo_gender);
      googletag.pubads().setTargeting("age", mi__dart_demo_age);
      googletag.pubads().setTargeting("income", mi__dart_demo_income);
    }
  });

//]]>
    </script>
