              <script type="text/javascript">
//<![CDATA[

&lt;!--

function searchAjax(ajaxSearchType, activeId) {

	// check if we can expand the search type
	if (searchType == 'all') {
		if (ajaxSearchType == 'news') {
			return showMoreNewsResults();
		} else if (ajaxSearchType == 'site') {
			return showMoreSiteResults();
		}  
	}
	var bgColor = '#D5EEF7';
	$('#bw-all-nav').animate({ backgroundColor: bgColor }, 300).removeClass("bw-current");
	$('#bw-news-nav').animate({ backgroundColor: bgColor }, 300).removeClass("bw-current");
	$('#bw-news-photos-nav').animate({ backgroundColor: bgColor }, 300).removeClass("bw-current");
	$('#bw-news-videos-nav').animate({ backgroundColor: bgColor }, 300).removeClass("bw-current");
	$('#bw-site-nav').animate({ backgroundColor: bgColor }, 300).removeClass("bw-current");

	searchTerm = $('#bw-search-page-input').val();
	var searchUrl = '/portal/site/home/template.BINARYPORTLET/search/resource.process/?javax.portlet.tpst=92055fbcbec7e639f1f554100d908a0c&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchTerm=SEARCH_TERM&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_resultsPerPage=10&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_resultsPage=1&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchType=SEARCH_TYPE&amp;javax.portlet.rid_92055fbcbec7e639f1f554100d908a0c=searchAjax&amp;javax.portlet.rcl_92055fbcbec7e639f1f554100d908a0c=cacheLevelPage&amp;javax.portlet.begCacheTok=com.vignette.cachetoken&amp;javax.portlet.endCacheTok=com.vignette.cachetoken'.replace('SEARCH_TERM', searchTerm).replace('SEARCH_TYPE', ajaxSearchType);

	searchType = ajaxSearchType;
	searchPage = 1;
	
	if (!/\S/.test(searchTerm)) { // check for blank
		$('.bw-search-results').html('&lt;br/&gt;&lt;p&gt;Please enter a search term.&lt;p&gt;');
		updateNavIndicator(searchType);
	} else {
		$(".bw-search-results").animate({'opacity':0.5});
	
		$.ajax({
			  url: searchUrl,
			  success: function( data ) {
			    $('.bw-search-results').replaceWith(data);
			    updateNavIndicator(searchType);
				$(".bw-search-results").animate({'opacity':1.0});
			  }
			});

	}
	History.pushState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
	return false;
}

function updateNavIndicator(searchType) {
	var activeId = '';
	if (searchType == 'all') {
		activeId = 'bw-all-nav';
	} else if (searchType == 'site') {
		activeId = 'bw-site-nav';
	} else if (searchType == 'news' || searchType == 'headline' || searchType == 'company' || searchType == 'ticker' || searchType == 'isin') {
		activeId = 'bw-news-nav';
	} else if (searchType == 'newsPhotos') {
		activeId = 'bw-news-photos-nav';
	} else if (searchType == 'newsVideos') {
		activeId = 'bw-news-videos-nav';
	}

	var bgColor = '#D5EEF7';
	$('#bw-all-nav').css({ backgroundColor: bgColor }).removeClass("bw-current");
	$('#bw-news-nav').css({ backgroundColor: bgColor }).removeClass("bw-current");
	$('#bw-news-photos-nav').css({ backgroundColor: bgColor }).removeClass("bw-current");
	$('#bw-news-videos-nav').css({ backgroundColor: bgColor }).removeClass("bw-current");
	$('#bw-site-nav').css({ backgroundColor: bgColor }).removeClass("bw-current");
	
	$('#' + activeId).animate({ backgroundColor: "#499ED6" }, 300, function() {
		$(this).addClass("bw-current");
	});

}

//--&gt;

//]]>
              </script>
