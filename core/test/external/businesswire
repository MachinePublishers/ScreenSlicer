//<![CDATA[

&lt;!--
	$('.bw-site-highlight').css("background-color","#FFFFCC").css("border","2px solid yellow");
	setTimeout(function () { 
		$('#bw-search-page-input').focus();
	}, 100);
//--&gt;

//]]>
              </script>
            </div>
            <!-- end .bw-content -->            <script type="text/javascript">
//<![CDATA[


searchType = 'all'; 
searchTerm = 'ibm'; 
searchPage = 1;

(function(window,undefined){

    var History = window.History; 
    if (!History.enabled) {
        return false;
    }
    //History.debug.enable = true;
 	var state = History.getState(); 

    // Restore Initial State if present
	History.debug('initial:', state.data, state.title, state.url);
	
    if (typeof state.data.searchTerm != 'undefined') {
    	stateSearchType = state.data.searchType;
		stateSearchTerm = state.data.searchTerm;
		statePage = state.data.page;
	    History.debug("Setting initial state: SearchType: " + stateSearchType + ", searchTerm: " + stateSearchTerm + ", page: " + statePage);
		setPageState(stateSearchType, stateSearchTerm, statePage);
    	History.replaceState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
    } else {
    	History.debug("no initial state");
    	
    	// check for url params
		stateSearchType = getParameterByName('searchType');
		stateSearchTerm = getParameterByName('searchTerm');
		statePage = getParameterByName('searchPage');
	    if (stateSearchType != null &amp;&amp; stateSearchTerm != null &amp;&amp; statePage != null) {
    		// set them here
		    History.debug("initial params: SearchType: " + stateSearchType + ", searchTerm: " + stateSearchTerm + ", page: " + statePage);
			setPageState(stateSearchType, stateSearchTerm, statePage);
	    }  
	    	
	    if (searchType != 'all' || searchTerm != '' || searchPage != 1) {
	    	// replace the state unless default
	    	History.replaceState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
	    }
		
    }
	
    // Bind to StateChange Event
    History.Adapter.bind(window,'statechange',function(){
        var state = History.getState();
			
		var stateSearchType = state.data.searchType;
		var stateSearchTerm = state.data.searchTerm;
		var statePage = state.data.page;

	    History.debug("statechange: SearchType: " + stateSearchType + ", searchTerm: " + stateSearchTerm + ", page: " + statePage);

        if (typeof state.data.searchType == 'undefined' &amp;&amp; typeof state.data.searchTerm == 'undefined' &amp;&amp; typeof state.data.page == 'undefined') {
			if (typeof searchType != 'undefined' &amp;&amp; typeof searchTerm != 'undefined' &amp;&amp; typeof searchPage != 'undefined') {
				return;
			}
			setPageState('all', '', 1);
        } else if (searchType != stateSearchType || searchTerm != stateSearchTerm || searchPage != statePage) {
			History.debug("page state doesn't match History state, setting page state...");

        	setPageState(stateSearchType, stateSearchTerm, statePage);
        } else {
        	History.debug("states match, internal call");
        }

        History.debug(state.data, state.title, state.url);
    });

})(window);

function getParameterByName(name) {
    var match = RegExp('[?&amp;]' + name + '=([^&amp;]*)').exec(window.location.search);
    return match &amp;&amp; decodeURIComponent(match[1].replace(/\+/g, ' '));
}


function searchBox() {
	
	searchTerm = $('#bw-search-page-input').val();
	searchPage = 1;
	
	if (!/\S/.test(searchTerm)) { // check for blank
		$('.bw-search-results').html('&lt;br/&gt;&lt;p&gt;Please enter a search term.&lt;p&gt;');
	} else {
		var searchUrl = '/portal/site/home/template.BINARYPORTLET/search/resource.process/?javax.portlet.tpst=92055fbcbec7e639f1f554100d908a0c&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchTerm=SEARCH_TERM&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchType=SEARCH_TYPE&amp;javax.portlet.rid_92055fbcbec7e639f1f554100d908a0c=searchBox&amp;javax.portlet.rcl_92055fbcbec7e639f1f554100d908a0c=cacheLevelPage&amp;javax.portlet.begCacheTok=com.vignette.cachetoken&amp;javax.portlet.endCacheTok=com.vignette.cachetoken'.replace('SEARCH_TERM', searchTerm).replace('SEARCH_TYPE', searchType).replace('RESULTS_PAGE', searchPage);
		
		$(".bw-search-results").animate({'opacity':0.5});
		$("#bw-search-page-input").prop('disabled', true);
		
		$.ajax({
			  url: searchUrl,
			  success: function( data ) {
			    $('.bw-search-results').replaceWith(data);
			    $(".bw-search-results").animate({'opacity':1.0});
				$("#bw-search-page-input").prop('disabled', false);
			  }
			});
	}
	
	History.pushState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
	return false;
}

function pageResults(page) {
		searchPage = page;
		var searchUrl = '/portal/site/home/template.BINARYPORTLET/search/resource.process/?javax.portlet.tpst=92055fbcbec7e639f1f554100d908a0c&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchTerm=SEARCH_TERM&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_resultsPage=RESULTS_PAGE&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchType=SEARCH_TYPE&amp;javax.portlet.rid_92055fbcbec7e639f1f554100d908a0c=searchBox&amp;javax.portlet.rcl_92055fbcbec7e639f1f554100d908a0c=cacheLevelPage&amp;javax.portlet.begCacheTok=com.vignette.cachetoken&amp;javax.portlet.endCacheTok=com.vignette.cachetoken'.replace('SEARCH_TERM', searchTerm).replace('SEARCH_TYPE', searchType).replace('RESULTS_PAGE', page);
		
		$(".bw-search-results").animate({'opacity':0.5});
		$("#bw-search-page-input").prop('disabled', true);
		
		$.ajax({
			  url: searchUrl,
			  success: function( data ) {
			    $('.bw-search-results').replaceWith(data);
			    $(".bw-search-results").animate({'opacity':1.0});
				$("#bw-search-page-input").prop('disabled', false);
				$("html, body").animate({ scrollTop: 0 }, 600);
			  }
			});

		History.pushState({'searchType':searchType, 'searchTerm':searchTerm, 'page':page}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
		return false;
}

function showMoreSiteResults() {
	if (searchType == 'site') {
		return false;
	}
	
	searchType = 'site';
	$('#bw-more-site-list').css("margin-top","0px"); 
	$('.bw-site-div-hidden').show("blind", { direction: "vertical" }, 600);
	$('.bw-news-section').hide("blind", { direction: "vertical" }, 600);
	$('#more-site-results').delay(500).fadeOut(200);
	$('#bw-all-nav').animate({ backgroundColor: "#d5eef7" }, 300).removeClass("bw-current");
	$('#bw-news-nav').animate({ backgroundColor: "#d5eef7" }, 300).removeClass("bw-current");
	$('#bw-site-nav').delay(300).animate({ backgroundColor: "#499ED6" }, 300, function() {
		$(this).addClass("bw-current");
	});
	
	pushState = true;
	History.pushState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
	return false;
}

function showMoreNewsResults() {
	if (searchType == 'news') {
		return false;
	}

	$('#bw-more-news-list').css("margin-top","0px"); 
	$('.bw-news-div-hidden').show("blind", { direction: "vertical" }, 600);
	$('.bw-site-section').hide("blind", { direction: "vertical" }, 600);
	$('#more-news-results').delay(100).fadeOut(200);
	$('#more-news-pagination').css("margin-top","0px").delay(300).fadeIn(200);

	$('#bw-all-nav').animate({ backgroundColor: "#d5eef7" }, 300).removeClass("bw-current");
	$('#bw-site-nav').animate({ backgroundColor: "#d5eef7" }, 300).removeClass("bw-current");
	$('#bw-news-nav').delay(300).animate({ backgroundColor: "#499ED6" }, 300, function() {
		$(this).addClass("bw-current");
	});

	$("html, body").animate({ scrollTop: 0 }, 600);
	
	searchType = 'news';

	pushState = true;
	History.pushState({'searchType':searchType, 'searchTerm':searchTerm, 'page':searchPage}, "Search | Business Wire", "?searchType=" + searchType + "&amp;searchTerm=" + searchTerm + "&amp;searchPage=" + searchPage);
	return false;
}

function setPageState(type, term, page) {
	
	History.debug("Setting Page State: SearchType: " + type + ", term: " + term + ", page: " + page);
	
	// set the page to match
	searchType = $.trim(type);
	searchTerm = $.trim(term);
	searchPage = $.trim(page);

	// set the input box to show searchTerm 
	$('#bw-search-page-input').val(term);
	
	if (!/\S/.test(searchTerm)) { // check for blank
		$('.bw-search-results').html('&lt;br/&gt;&lt;p&gt;Please enter a search term.&lt;p&gt;');
		updateNavIndicator(type);
	} else {
		var searchUrl = '/portal/site/home/template.BINARYPORTLET/search/resource.process/?javax.portlet.tpst=92055fbcbec7e639f1f554100d908a0c&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchTerm=SEARCH_TERM&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_resultsPage=RESULTS_PAGE&amp;javax.portlet.rst_92055fbcbec7e639f1f554100d908a0c_searchType=SEARCH_TYPE&amp;javax.portlet.rid_92055fbcbec7e639f1f554100d908a0c=searchBox&amp;javax.portlet.rcl_92055fbcbec7e639f1f554100d908a0c=cacheLevelPage&amp;javax.portlet.begCacheTok=com.vignette.cachetoken&amp;javax.portlet.endCacheTok=com.vignette.cachetoken'.replace('SEARCH_TERM', term).replace('SEARCH_TYPE', type).replace('RESULTS_PAGE', page);
	
		$(".bw-search-results").animate({'opacity':0.5});
		$("#bw-search-page-input").prop('disabled', true);
	
		// remove the current highlight during the search
		$('#bw-all-nav').animate({ backgroundColor: "##d5eef7" }, 300).removeClass("bw-current");
		$('#bw-news-nav').animate({ backgroundColor: "##d5eef7" }, 300).removeClass("bw-current");
		$('#bw-news-photos-nav').animate({ backgroundColor: "##d5eef7" }, 300).removeClass("bw-current");
		$('#bw-news-videos-nav').animate({ backgroundColor: "##d5eef7" }, 300).removeClass("bw-current");
		$('#bw-site-nav').animate({ backgroundColor: "##d5eef7" }, 300).removeClass("bw-current");
		
		$.ajax({
		  url: searchUrl,
		  success: function( data ) {
		    $('.bw-search-results').replaceWith(data);
		    $(".bw-search-results").animate({'opacity':1.0});
			$("#bw-search-page-input").prop('disabled', false);
			$("html, body").animate({ scrollTop: 0 }, 600);
			
			// set the nav indicator
			updateNavIndicator(type);
		  }
		});
	}
}

//--&gt;

//]]>
