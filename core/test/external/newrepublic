    <script type="text/javascript">
//<![CDATA[

var scroller = false;
var callbackDelegates = [];

// In touch cases we can't rely on the page offset to determine where we are, nor can we bind with the body scrolling. So we need to register anything that needs to know about such things with the scroller.
var throttledUpdateScrollDelegates = jQuery.throttle(function(){
  for(var i = 0; i &lt; callbackDelegates.length; i++){
    callbackDelegates[i].updateWithNewScrollPosition(-scroller.y);
  }
},  100);

// Only do this when we're in a touch environment, which we'll take to mean a mobile device and has trouble with
// fixed position objects, hence all this tremendous hoop jumping.
function pageLoadedForScrolling() {
  if(isTouchCase()){

    var mobileFlyout;
    if(jQuery('#mobile-flyout').length &gt; 0){
      mobileFlyout = jQuery('#mobile-flyout');
    } else {
      mobileFlyout = false;
    }
    scroller = new iScroll('wrapper', {
      hScroll: false,
      hScrollbar: false,
      hideScrollbar: false,
      bounce: false,
      onScrollMove: function(){
        throttledUpdateScrollDelegates();
        if(mobileFlyout){
          mobileFlyout.data('plugin_tnrMobileFlyout').contentIsScrolling();
          mobileFlyout.data('plugin_tnrMobileFlyout').hide();
        }
      },
      onTouchEnd: function(){
        if(mobileFlyout){
          mobileFlyout.data('plugin_tnrMobileFlyout').hide();
        }
      },
      onScrollEnd: function(){
        throttledUpdateScrollDelegates();
        if(mobileFlyout){
          mobileFlyout.data('plugin_tnrMobileFlyout').contentIsNotScrolling();
        }
      }
    });
    marginaliaMayhem();
  }
}

function tempHideMarginaliaOnMobile() {
  if(Modernizr.mq('only screen and (max-width: 69em)')){
    if(Modernizr.touch){
      jQuery('#marginalia').css('display','none');
      jQuery('#marginalia').css('width','0');
      jQuery('#marginalia').css('visibility','hidden');
      jQuery('#marginalia-scroller .handle').css('display','none');
      jQuery('#marginalia-clone .handle').css('display','none');
      jQuery('.messages').css('display','none');
    }
  }
}

// if (typeof pageLoadedForScrolling == 'function') {
// 	if (document.addEventListener){
// 	  document.addEventListener('DOMContentLoaded', function () { setTimeout(pageLoadedForScrolling, 200); }, false);
// 	} else if (document.attachEvent){
// 	  document.attachEvent('DOMContentLoaded', function () { setTimeout(pageLoadedForScrolling, 200); }, false);
// 	}
// }


// Delegate .transition() calls to .animate()
// if the browser can't do CSS transitions, for jquery.transit.
if (!jQuery.support.transition)
    jQuery.fn.transition = jQuery.fn.animate;


var modalSlideshow;
var AUDIO_QUEUE;  // Make it a global so we can access it elsewhere.
var marginaliaSlider = false; // Used when we've activated the scroller in the marginalia.

jQuery(document).ready(function($) {

  // The sidebars are only in shelf mode in narrower screens.
  $('#other').tnrShelfControls({});
  $('#marginalia').tnrShelfControls({});

  $('.controls-list').tnrFlyoutController({});

  $('#receive-print-edition').change(function(e){
    if($(this).is(':checked')){
      $('#shipping-address-fieldset').show();
    } else {
      $('#shipping-address-fieldset').hide();
    }
    e.preventDefault();
  })

  // Only need this when the user is logged in.
  AUDIO_QUEUE = $('#audio-queue').tnrAudioQueue({
    triggerButtonElement: $('#logged-in-listen') // Watch the headphones element, it shows the flyout.
  });
  $('#audio-queue').niceScroll({ // Make the audio queue widget scrollable.
    autohidemode: false,
    cursorcolor: '#8c8c8c'
  });

  // Only enable the highlighter on the main section of .content .pages.
  // Drupal.settings.tnrUserSettings.tnrShareHighlight = $user-&gt;share_highlight
  // If setting is not initialized or turned on for user, don't use highlighter
  if (window.getSelection){ // And only if the browser can hang.
    if (typeof Drupal.settings.tnrUserSettings != 'undefined') {
      if (Drupal.settings.tnrUserSettings.tnrShareHighlight == '1') {
        return;
      }
    }
    $('.page.content').find('.main').highlighter({'selector':'#highlight-overlay', 'minWords': 1});
  }

  // Hide the marginalia until we're ready to show it.
  $('#marginalia').find('.item').css('visibility', 'hidden');

  // We show a few elements once another element has scrolled away.
  // We want the marginalia controls to be the trigger, but they might not exist.
  // If they don't we'll use the title.
  var marginaliaControls = $('#controls');
  if(marginaliaControls.length &gt; 0 &amp;&amp; marginaliaControls.css('display') != 'none'){
    triggerElement = marginaliaControls;
  } else {
    triggerElement = $('#primary-title');
  }

  if($("#back-to-top").length &gt; 0 &amp;&amp; triggerElement.length &gt; 0){
    var title = $('#primary-title');
    $("#back-to-top").backToTopWidget({
      offsetTriggerPosition: triggerElement.offset().top + triggerElement.height(), // When we scroll beyond this we'll show the widget.
      leftElement: $('.main')
    });
  }

  if($('#fixed-header-container').length &gt; 0 &amp;&amp; triggerElement.length &gt; 0){
    $('#fixed-header-container').tnrFixedHeader({
      offsetTriggerPosition: triggerElement.offset().top + triggerElement.height() // When we scroll beyond this we'll show the widget.
    });
    callbackDelegates.push($('#fixed-header-container').data('plugin_tnrFixedHeader'));
  }


  $('#discourse').tnrArticleComments({});

  // Watch that Other column promo closers.
  $('#other').find('.promo a.close').click(function(e){
    $(this).closest('.promo').hide();
    e.preventDefault();
  });
});


(function ($) {
  $(window).load(function() {

    // Some things can only be done once the page has rendered, because we need to know its final dimensions.

    // The progress bar in article titles.
    // Only do this on article pages.
    if($('#page').hasClass('article')){
      $('#title-progress-bar-container').tnrProgressBar({    maxWidthElement: $('#main') });
      // The sticky header progress bar.
      if($(window).width() == $('#fixed-header').width()){ // iPhone mode is full width.
        $('#fixed-header-progress-bar-container').tnrProgressBar({
          maxWidthOffset: -3,
          maxWidthElement: $(window)
        });
      } else {
        $('#fixed-header-progress-bar-container').tnrProgressBar({
          maxWidthOffset: 28,
          maxWidthElement: $('#main')
        });
      }
      // If this is a touch environment we'll be using iscroll, so we need to wire up the fixed header
      // progress bar to receive its position updated.
      callbackDelegates.push($('#fixed-header-progress-bar-container').data('plugin_tnrProgressBar'));
    }

    // Once the page has loaded we can position the marginalia intellegently.
    positionMarginalia();
    observeFootnoteEvents();   // Watch for footnote clicks.
    tempHideMarginaliaOnMobile();

    if (scroller != false) {
      scroller.refresh();
    }
  }).resize(function(){
      positionMarginalia(); // On window resize we'll need to adjust the marginalia.
  });

})(jQuery);

//]]>
    </script>
    <meta content="{&quot;title&quot;:&quot;Search&quot;,&quot;link&quot;:&quot;search\/site\/us&quot;,&quot;type&quot;:&quot;sectionpage&quot;}" name="parsely-page"/>
