					ad_ord = Math.floor(9999999999 * Math.random()),
					ad_seg = "",
					ad_mnr = -1 &lt; document.cookie.indexOf("grif=1") ? "t" : "f",
					ad_ref = "other",
					ref = document.referrer,
					refsMap = {"facebook.com": "facebook", "twitter.com|t.co": "twitter", "plus.url.google.com|plus.google.com": "googleplus", "google.com": "google", "bing.com": "bing", "yahoo.com": "yahoo", "espn.go.com": "espn"},
					refKey;
			for (refKey in refsMap) {
				ref.match(refKey) &amp;&amp; (ad_ref = refsMap[ refKey ]);
			}
