						if ("undefined" == typeof ad_site || "undefined" == typeof ad_zone) {
							var ad_site = "espn.fivethirtyeight.com",
									ad_zone = "story";
						}
						var ad_finalString = "http://ad.doubleclick.net/N6444/adj/" + ad_site + "/" + ad_zone + ";" + ad_kvps + "pos=PresentedByLogo;" + ad_u + "sz=200x24,1x1;tile=" + ad_counter + ";ord=" + ad_ord;
						document.write('&lt;div data-cb-ad-id="PresentedByLogo" data-cb-dfp-id="' + ad_kvps + 'sz=200x24,1x1"&gt;&lt;script src=' + ad_finalString + '&gt;\x3c/script&gt;&lt;/div&gt;');
						ad_counter++
					}

							
//]]>
            </script>
            <div data-cb-dfp-id="sp=espn;ref=twitter;mnr=f;sz=200x24,1x1" data-cb-ad-id="PresentedByLogo">
              <script src="http://ad.doubleclick.net/N6444/adj/espn.fivethirtyeight.com/search;sp=espn;ref=twitter;mnr=f;pos=PresentedByLogo;sz=200x24,1x1;tile=2;ord=8065082009">
              </script>
            </div>
            <noscript>
              &lt;a href="http://ad.doubleclick.net/jump/espn.fivethirtyeight.com/search;sp=espn;sz=200x24,1x1;" target="_blank"&gt;
					&lt;img src="http://ad.doubleclick.net/ad/espn.fivethirtyeight.com/search;sp=espn;sz=200x24,1x1;" border="0" alt="" /&gt;
				&lt;/a&gt;
			
            </noscript>
          </div>
