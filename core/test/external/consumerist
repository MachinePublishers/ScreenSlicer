//<![CDATA[

function janrainSignOut(){
	janrain.capture.ui.endCaptureSession();
}
(function() {
	if (typeof window.janrain !== 'object') window.janrain = {};
	window.janrain.settings = {};
	window.janrain.settings.capture = {};

	// capture settings
	janrain.settings.capture.redirectUri = 'http://consumermediallc.wordpress.com/wp-admin/admin-ajax.php?action=janrain_capture_redirect_uri';
	janrain.settings.capture.appId= '977s6g79b4gtprppc8eud5wjbg';
	janrain.settings.capture.clientId = 'jvwzj44vgqjr6ngcknqjd2a68cq43zve';
	janrain.settings.capture.responseType = 'token';
	janrain.settings.capture.captureServer = 'https://consumerist.janraincapture.com';
	janrain.settings.capture.registerFlow = 'socialRegistration';
	janrain.settings.packages = ['capture'];

	janrain.settings.capture.setProfileCookie = true;
	janrain.settings.capture.keepProfileCookieAfterLogout = true;
	janrain.settings.capture.setProfileData = 'true';

	// styles
	janrain.settings.capture.stylesheets = ['http://s1.wp.com/wp-content/themes/vip/consumerist2/janrain-capture-screens/stylesheets/styles.css?m=1375308127g'];
	janrain.settings.capture.mobileStylesheets = ['http://s1.wp.com/wp-content/themes/vip/consumerist2/janrain-capture-screens/stylesheets/mobile-styles.css?m=1375308127g'];
	janrain.settings.capture.recaptchaPublicKey = '6LeVKb4SAAAAAGv-hg5i6gtiOV4XrLuCDsJOnYoP';

	//backplane settings
	janrain.settings.capture.backplane = 1;
	janrain.settings.capture.backplaneBusName = 'consumerist';
	janrain.settings.capture.backplaneVersion = 1.2;
		
	function isReady() { janrain.ready = true; };
	if (document.addEventListener) {
		document.addEventListener("DOMContentLoaded", isReady, false);
	} else {
		window.attachEvent('onload', isReady);
	}

	var e = document.createElement('script');
	e.type = 'text/javascript';
	e.id = 'janrainAuthWidget'
	var url = document.location.protocol === 'https:' ? 'https://' : 'http://';
	url += 'widget-cdn.rpxnow.com/load/.default';
	e.src = url;
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(e, s);
})();

//]]>
    </script>
    <script type="text/javascript">
//<![CDATA[
function janrainCaptureWidgetOnLoad() {
	janrain.settings.capture.returnExperienceUserData = ['uuid', 'displayName'];

	janrain.events.onCaptureSessionNotFound.addHandler( function( result ) {
		eraseCookie( 'janrain-logged-in' );
		eraseCookie( 'janrain-username' );

		jQuery( document.getElementById( 'respond' ) ).remove();
		jQuery( document.querySelectorAll( '.comment-edit-link' ) ).remove();
		jQuery( document.querySelectorAll( '.comment-reply-link' ) ).remove();
	});

	janrain.events.onCaptureSessionFound.addHandler( function( result ) {
		var username  	 = readCookie( 'janrain-username' ),
			logged_in 	 = readCookie( 'janrain-logged-in' ),
			access_token = result.accessToken;

		if ( null === username || 'undefined' === username || username !== janrain.capture.ui.getReturnExperienceData( 'displayName' ) ) {
			var name = janrain.capture.ui.getReturnExperienceData( 'displayName' );

			if ( null !== name &amp;&amp; 'undefined' !== name ) {
				createCookie( 'janrain-username', name, 1 );

				if ( null === logged_in || 'undefined' === logged_in ) {

					if ( null !== access_token &amp;&amp; 'undefined' !== access_token ) {
						// set up our data we will pass to our script
						var data = {
							action: 'consumerist_encrypt_token',
							consumerist_encrypt_token: consumerist_ajax.encrypt_token_nonce,
							janrain_token: access_token,
							janrain_username: name
						};

						// consumerist_ajax.ajaxurl is a variable that will contain the url to the ajax processing file
						jQuery.ajax( {
							type: 'POST',
							url: consumerist_ajax.ajaxurl,
							data: data,
							xhrFields: {
								withCredentials: true
							},
							success: function( response ) {
								if ( response &amp;&amp; 'fail' !== response ) {
									createCookie( 'janrain-logged-in', response, 1 );

									// reload page so server side code can take effect.
									window.location.reload( true );
								}
							}
						} );
					}
				}
			} else {
				eraseCookie( 'janrain-logged-in' );
				eraseCookie( 'janrain-username' );
			}
		}

		jQuery('#commentform').on( 'submit', function(e) {
			if ( localStorage &amp;&amp; ! localStorage.getItem( 'janrainCaptureToken' ) ) {
				jQuery('#commentform p.error').removeClass('hidden');
				return false;
			}
		});
	});

	function passwordValidation(name, value) {
    	return /([a-zA-Z].*[!@#$%^&amp;*_-].*)|([0-9].*[a-zA-Z+!@#$%^&amp;*_-].*)|([.!@#$%^&amp;*_-].*[a-zA-Z0-9].*)/.test(value);
	}

    janrain.events.onCaptureLoginFailed.addHandler(function(result){
        if (result.statusMessage == "accountDeactivated") {
            alert("Your account has been deactivated.");
            janrain.capture.ui.modal.close();
        }
    });

    janrain.events.onCaptureLoginSuccess.addHandler( function( result ) {
		// set up our data we will pass to our script
		var data = {
			action: 'consumerist_encrypt_token',
			consumerist_encrypt_token: consumerist_ajax.encrypt_token_nonce,
			janrain_token: result.accessToken,
			janrain_username: result.userData.displayName
		};

		// consumerist_ajax.ajaxurl is a variable that will contain the url to the ajax processing file
		jQuery.ajax( {
			type: 'POST',
			url: consumerist_ajax.ajaxurl,
			data: data,
			xhrFields: {
				withCredentials: true
			},
			success: function( response ) {
				if ( response &amp;&amp; 'fail' !== response ) {
					createCookie( 'janrain-username', result.userData.displayName, 1 );
					createCookie( 'janrain-logged-in', response, 1 );
				}

				location.reload();
			}
		} );
	});

    janrain.events.onCaptureRegistrationSuccess.addHandler(function(result){location.reload()});
    janrain.capture.ui.registerFunction('passwordValidation', passwordValidation);
    janrain.capture.ui.start();
}


//]]>
    </script>
