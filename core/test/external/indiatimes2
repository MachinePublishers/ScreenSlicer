					}
				//} 
			}else if (e.keyCode == 39 &amp;&amp; $(document.activeElement).attr("type") != "textarea"   &amp;&amp;  $(document.activeElement).attr("type") != "text") {    
				//if(auto_play == 0 || auto_play == null ){	
					if($('.next').attr('href')){        
						window.location = $('.next').attr('href');
						return false;
					}
				//}	  
			}
		});
	})

//]]>
        </script>
      </div>
