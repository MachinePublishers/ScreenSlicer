                  <script type="text/javascript">
//<![CDATA[

    var INTEL_TYPE_AHEAD = {};
    $(function (){
        var t = INTEL_TYPE_AHEAD,
            setProtocol = function(txt){
                return txt.replace('http:', location.protocol);
            },
            embed = 'Target Audience/Embedded Developers &amp; Engineers',
            isEmbed = $.trim($('#toplevelcategory').val()) == embed;
        t.langRootPath = '/content/www/us/en';
        t.headingTxt = 'Best Match';
        t.typeAheadUrl = setProtocol('http://search.intel.com/SearchLookup/DataProvider.ashx');
        t.locale = 'en_US';
        t.bestMatchUrl = setProtocol('http://search.intel.com/webhandlers/searchwebhandler.ashx');
        t.typeAheadDefaults = {
            m: 'GetTypeAheadSuggestions',
            languageCode: 'en_US',
            includeBestMatch: true,
            searchRealm: 'Default'
        };
        t.bestMatchDefaults = {
            q1: 'reimagine',
            q2: 'en',
            q3: '1',
            q11: 'title,description,url,thumbnailurl',
            q14: 'filter(localecode:string("en_US") ANDNOT generic1:"false" ANDNOT (reimaginerootlevel:"Social@Intel") ' + (isEmbed ? 'AND' : 'ANDNOT') + ' (reimaginetoplevelcategory:"' + embed + '"))',
            q23: '^',
            q28: 'true',
            q30: 'yes'
        };
        t.cloneParams = function(obj, overrides){
            var newObj = {};
            for (var i in obj){
                newObj[i] = obj[i];
            }
            for (var i in overrides){
                newObj[i] = overrides[i];
            }
            return newObj;
        };
        t.loadMenuAsync = function(){
            var po = document.createElement('script');
            po.type = 'text/javascript';
            po.async = true;
            po.src = '/etc/designs/ver/4.5.14.4.13/intel/us/en/clientlibs/predictive-search.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(po,s);
        }();

        $('#searchBox').keyup(function(event){
            var $this = $(this),
                    searchVal = $this.val(),
                    inputs = {};
            if(event.keyCode != 13){
                inputs.params = t.cloneParams(t.typeAheadDefaults, { searchPhrase: searchVal });
                inputs.bestMatchParams = t.cloneParams(t.bestMatchDefaults, { q10: 'corporatecompositefield^' + searchVal + '^none' });
                inputs.box = $this;
                inputs.suffix = '';
                t.getTypeAhead(inputs);
            }
        });
    });

//]]>
                  </script>
                </div>
