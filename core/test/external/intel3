        <div data-component-id="1" data-component="uheader" class="component">
          <script>
//<![CDATA[

    if ((document.cookie.indexOf('SMSESSION') != -1) &amp;&amp; (location.protocol == "http:")) {
        window.location = "https://www-ssl.intel.com/content/www/us/en/search.html" + location.search;
    }

