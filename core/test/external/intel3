          <script type="text/javascript">
//<![CDATA[

    $(function () {
        var signinURL = "/content/www/us/en/homepage/_jcr_content/header.signin.html?redirect=/content/www/us/en/search.html&amp;location=US&amp;lang=en";
        $(".sign-in-link .link a").bind("click", function (e) {
            e.preventDefault();
            if (!$(this).hasClass("selected")) {
                $("#sign-in-form").load(signinURL, function () {
                    $(".signin-nav-box").slideDown(function () {
                        $(".sign-in-link .link a").addClass("selected");

                    });
                });
            } else {
                closeSigninBox();
            }
        });
    });

//]]>
          </script>
