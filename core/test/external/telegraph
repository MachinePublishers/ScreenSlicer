//<![CDATA[

(function(){
  if (window.Krux) {
    var kuid = window.Krux('get', 'user');
    if (kuid) {
       var rubicon_url = '//tap.rubiconproject.com/oz/feeds/krux/tokens?afu=' + kuid;
