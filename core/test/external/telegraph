    var queryText = "ibm";
    
        try {
            function replaceAll(text, strA, strB)
            {
                while ( text.indexOf(strA) != -1)
                {
                    text = text.replace(strA,strB);
                }
                return text;
            }

            var queryTextRemovePlus = replaceAll(queryText, "+", " ");            
            var queryTextRemoveQuotes = unescape(replaceAll(queryTextRemovePlus, "%22", ""));

