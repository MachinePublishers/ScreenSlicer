      <span class="kxtag kxinvisible" data-id="27199" data-alias="Rubicon Segment Sharing">
        <script type="text/javascript">
//<![CDATA[

(function() {
  var segs = Krux('get', 'user_prvx_segments') || [];
  if (segs.length) {
    var config = Krux('get', 'config');
    if (config) {
      var partner_segment_map = config['partner_segment_map'];
      if (partner_segment_map) {
        rubicon_segs = [];
        for (var i = 0; i < segs.length; i++) {
          var seg_map_data = partner_segment_map[segs[i]];
          if (seg_map_data) {
            rseg = seg_map_data['rubicon'];
            if (rseg) {
              rubicon_segs.push(rseg)
            }
          }          
        }
        if (rubicon_segs.length) {
            (new Image()).src = '//pixel.rubiconproject.com/tap.php?v=' + rubicon_segs.join('||');
        }
      }         
    }
  }
})();

//]]>
        </script>
      </span>
      <span class="kxtag kxinvisible" data-id="22646" data-alias="Krux AS Transfer Code">
        <script>
//<![CDATA[

var scripts = document.getElementsByTagName('script');
var asiSegmentValues = [];

for(var i = 0; i < scripts.length; i++){
    var srcUrl = scripts[i].getAttribute('src');
    if(srcUrl && srcUrl.indexOf('http://ad.doubleclick.net') === 0){
        var dfpSrcUrl = srcUrl;
        var keyValPairs = dfpSrcUrl.split(";");
        for(var i = 0; i < keyValPairs.length; i++){
            if(keyValPairs[i] && keyValPairs[i].indexOf('as=') === 0){
               var asiVals = keyValPairs[i].split("=");
               asiSegmentValues.push(asiVals[1]); 
            }
        }
        break;
     }
}

Krux('scrape', {
  page_attr_asiSegments: {
    javascript: 'asiSegmentValues'
  } 
});

//]]>
        </script>
      </span>
