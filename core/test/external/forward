var load_ads = 0;

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-7290892-1']);
  _gaq.push(['_setDomainName', 'forward.com']);


        load_human_stuff();
if (typeof jQuery != 'undefined') {
   $(window).one("mousemove", function() {
       setCookie('human', 1, 365*2);
   });
}



function load_ga() {
	if(ga_internal == 'yes') {
		_gaq.push(['_setCustomVar', 1, 'Internal', 'Forward', 2]);
	}

	

	if (typeof gaPageURL === 'undefined') {
		_gaq.push(['_trackPageview']);
	}
	else {
		_gaq.push(['_trackPageview', gaPageURL]);
	}


//blockmetrics
(function() {
       function async_load(){
           var protocol = ('https:' == document.location.protocol ? 'https://' : 'http://');
           var s = document.createElement('script');
           s.src = protocol + 'blockmetrics.com/static/adblock_detection/js/d.min.js';
           var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x);
       }
                
       bm_website_code = '31B55501921C4A96';
                    
       jQuery(document).ready(async_load);
   })();




// Google Analytics
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
} // end of load_ga()





function readCookie(name) {
   name += '=';
   for (var ca = document.cookie.split(/;\s*/), i = ca.length - 1; i != -1; i--) {
       if (!ca[i].indexOf(name)) {
           return ca[i].replace(name, '');
       }
   }
}

function load_human_stuff() {
   //load google analytics, clicky, quantcast, and ads
        load_ga();
}

function setCookie(c_name,value, exdays)
{
   var exdate=new Date();
   exdate.setDate(exdate.getDate() + exdays);
   var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
   document.cookie=c_name + "=" + c_value;
}



