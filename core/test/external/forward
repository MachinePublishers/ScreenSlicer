                $(function() {

                $.getJSON('/workspace/assets/scripts/most_emailed/most_emailed_get.php?item=read', function(data) {
                  var items = [];
                  var i = 1;
                  var handle = '';

                  $.each(data, function(key, val) {
                    handle = key.replace(/[^\w\d]/gi, "-");
                    items.push('&lt;li&gt;&lt;a href="' + val + '" onClick="_gaq.push([' + "'_trackEvent', 'ClickTracking', 'Sidebar_MostReadArticlesLinks', 'spot" + i++ + "'" + ']);"&gt;' + key + '&lt;/a&gt;&lt;/li&gt;');
                  });

                  $(items.join('')).appendTo('#mostread_list');

                });
                });
