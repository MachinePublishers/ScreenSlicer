        'lineHeightTitle': '18',
        'lineHeightDescription': '17',
        'lineHeightDomainLink': '19',
        'verticalSpacing': '9',
        'adLoadedCallback': ask.CSA.handleBottomAdsLoaded,
        'noAdLoadedCallback': ask.CSA.handleNoBottomAdsLoaded,
            'detailedAttribution' : true,        'clicktrackUrl': 'http://www.ask.com/clk?lpurl=http%3A%2F%2Fwww.ask.com%2Fweb%3Fq%3Dibm%26search%3D%26qsrc%3D364%26o%3D0%26l%3Ddir%26ad%3DdirN&c=a&qid=B3A29A9EDCA26A1D396F7BADB62C88DF&o=0&l=dir&pos=bottom&ip=4a886193&p_uid=&p_clk_id=&page=1&q=ibm&cl=aj-cat-sp14&qsrc=364&dqi=&ch=computers-other&lp_ref_url=0&pl=6f22Dm%3DB%25M%26y0i%3DnxSsLLz4o_XIK_pXJYccaJatIqbNG3Uz6%26Ct%3D9Lvr1%266Y%3Du3u97%25rLQ7stFkAM%2BuxQWV5%2BgHGSOlDZ%26gr%3DYYl0lL%265uq%3D%25Lz.9orL-bHjtN3%25pUnqLsjR4xukWtggNwUsbyepYF3%26EK%3DAu4yaXy2Qi1%26%25%2B%3D81WPf%26V6j%3Df%2B%26oS%3Ddee%26Cm%3Dr%26Bb%3D%261%3D3&up=1f0eK%3DFxreWh%26xe%3DifA%26F1.kHSxk2%3Dp5g%26Hm.w117v8%3DuC-ecu-AqQR%26SE.e3kw29IRQ8Xaq%3D8QLjEbbAZ-O54bX%26I%3D6YlMFCmvu&anxu=' + encodeURIComponent(window.location.href.split('?')[0])
    };

    window['backfillParam'] = window.backfillParam || {};
    window['backfillParam']['pl_hex'] = adblock1.colorBackground ? adblock1.colorBackground : "#FFFFFF";
    window['backfillParam']['pl_cl'] = pageOptions.pubId;
    window['backfillParam']['pl_ch'] = pageOptions.channel;
    window['backfillParam']['pl_type'] = 'csa';
    window['backfillParam']['pl_rp'] = (pageOptions.numRepeated) ? true : false;
    window['backfillParam']['pl_clksp'] = adblock1.clickableBackgrounds;
    window['backfillParam']['pl_rqnum'] = parseInt(adblock1.maxTop) + parseInt(adblock2.number);
    window['backfillParam']['pl_timeout'] = ask.CSA.showSerpTimeoutLength ? ask.CSA.showSerpTimeoutLength : 0;

    try {
        _googCsa('ads', pageOptions, adblock1, adblock2);
    } catch (err) {
        ask.CSA.showSerp();
    }
});



