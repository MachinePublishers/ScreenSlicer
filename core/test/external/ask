            'detailedAttribution' : true,        'clicktrackUrl': 'http://www.ask.com/clk?lpurl=http%3A%2F%2Fwww.ask.com%2Fweb%3Fq%3Dibm%26search%3D%26qsrc%3D364%26o%3D0%26l%3Ddir%26ad%3DdirN&c=a&qid=B3A29A9EDCA26A1D396F7BADB62C88DF&o=0&l=dir&pos=top&ip=4a886193&p_uid=&p_clk_id=&page=1&q=ibm&cl=aj-cat-sp14&qsrc=364&dqi=&ch=computers-other&lp_ref_url=0&pl=7f2f4t%3DGu6%26RzK%3DMP8NpRH7v_ESj_%2B68TOB1ONW%25t18VR7Aj%262f%3DD%25j0P%26Q0%3D1ZObL2uEPJq2awGNm3W82EWysqd%2BfU3Z%26aC%3DpuVB4D%26rky%3D%2B%25t.Rn4x-JtjWioKnA%25N3wi%25r%25uNfO84SNpLPWcqZkh%26yX%3DLQ3K2aO2yfI%26LV%3DBlZO6%267nK%3Dem%26LU%3DaEt%26r6%3De%26w2%3D%26j%3Dp&up=6683O%3D%25NZBQ%2B%26Ez%3DWsz%26FF.ySdyeI%3DOKz%26bu.anDDrf%3Dyf-MNi-3SyK%26Ni.NiqJ57EU66qny%3Doyr2zPFyQ-3EZjs%26H%3DEDRMx7&anxu=' + encodeURIComponent(window.location.href.split('?')[0])

    };

    var adblock2 = {
        'container': 'magic2',
        'number': '5',
        'width': '99.5%',
        'clickableBackgrounds': true,
            'longerHeadlines': true,            'colorBackground': '#FFFFFF',        'colorTitleLink': '#3300B5',
        'colorDomainLink': '#0E7744',
        'colorText': '#333333',
        'colorAttribution' : '#333333',
                    'fontSizeTitle': '15',
        'fontSizeDescription': '13',
        'fontSizeDomainLink': '13',
