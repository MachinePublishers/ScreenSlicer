                        FSR.CPPS.set('SEO',JASK.foresee.getCPPValue('SEO'));
                        FSR.CPPS.set('Concept_X',JASK.foresee.getCPPValue('CX'));
                        FSR.CPPS.set('Concept_X_Position',JASK.foresee.getCPPValue('CXP'));
                        FSR.CPPS.set('Concept_X_element',JASK.foresee.getCPPValue('CXE'));
                        FSR.CPPS.set('UNI_UID','388B2988-0596-46DB-B48A-0D8445E5A969');
                        FSR.CPPS.set('UNI_SID','668666103');
                        FSR.CPPS.set('UNI_PID','B3A29A9EDCA26A1D396F7BADB62C88DF');
                    }
                };

                var foreseeFlag = "0";

                if($j.cookie("fsf") == null && true == true){
                        $j.cookie("fsf", foreseeFlag, {path:'/', domain:"ask.com"});
                }

                if(($j.cookie("fsf") == '1' || false == true) && $j.cookie("fsr.r") == null) {
                                                                            $j.getScript(""+"/st/raw/ask/foresee/v1/foresee-trigger.js");                                                              }
            });
        
