$(function() {
    $( "#datepicker_start" ).datepicker({
        defaultDate: "",
        maxDate: "",
        altField: "#sdate",
        dateFormat: "yymmdd",
        onSelect: function( selectedDate ) {
            console.log('set');
            $( "#datepicker_end" ).datepicker( "option", "minDate", selectedDate );
        }
    });
    $( "#datepicker_end" ).datepicker({
        defaultDate: "",
        minDate: "",
        maxDate: "0",
        altField: "#edate",
        dateFormat: "yymmdd",
        onSelect: function( selectedDate ) {
            console.log('set');
            $( "#datepicker_start" ).datepicker( "option", "maxDate", selectedDate );
        }
    });
});
function resetForm() {
    $('#start').val('');
    $('#num').val('');
}
function closeAuthorLookup(author) {
    if (author == "" &amp;&amp; author != "") {  
        resetForm();
        $('#author').val('');
        $('#advancedSearch').submit();
    } else {
        $('#authorLookUp').hide();
        $('#filterAuthor').show();
    }
}
function checkQuery() {
    if ($('#searchQuery').val() == "") {  
        return;
    } else {
        resetForm();
        return;
    }
}

