    <script>
//<![CDATA[

var subNavClosed = true;
var openSub;
var ord = window.ord || Math.floor(Math.random() * 1e16);
var request;
var currentHeadlinesId;
$('.showSubNav').mouseover(function() {
    $(".main-nav ul li").removeClass("active");
    $("#"+this.id).parent().addClass("active");
    if ($("#"+this.id+"SubNav").css("display") == 'none') {
        if (subNavClosed) {
            navId = this.id;
            clearTimeout(openSub);
            openSub=setTimeout("openNav(navId);", 400);
        } else {
            $("#subnavWrapper .subNav").hide();
            $("#"+this.id+"SubNav").show();
        }
        if ($("#"+this.id+"SubNav div.first").html() == "") {
            listId = $("#"+this.id+"SubNav div.first").attr('id').replace("Headlines", "");
            if (typeof request != "undefined") {
                request.abort();
            }
            if ($("#"+listId+"Headlines").html() == "") {
                currentHeadlinesId = listId;
                request = $.ajax({
                    url: "/block?name=subnav-"+currentHeadlinesId
                }).done(function( html ) {
                    $("#"+$("#"+currentHeadlinesId).parent().parent().parent()[0].id + " .headlinesHolder").hide();
                    $("#"+currentHeadlinesId+"Headlines").html(html).show();
                });
            } else if (currentHeadlinesId != listId) {
                currentHeadlinesId = listId;
                $("#"+$("#"+currentHeadlinesId).parent().parent().parent()[0].id + " .headlinesHolder").hide();
                $("#"+currentHeadlinesId+"Headlines").show();
            }
            currentHeadlinesId = listId;
            $("#"+$("#"+currentHeadlinesId).parent().parent()[0].id+" li").removeClass("active");
            $("#"+currentHeadlinesId).parent().addClass("active");
            adContainer = $("#"+currentHeadlinesId).parent().parent()[0].id.replace("List", "");
        }
        $("#"+this.id+"Ad").html('&lt;iframe style="background: #DADADA;" src="http://ad.doubleclick.net/N5578/adi/nav_'+this.id+';sz=300x250;ord=' + ord + '?" width="300" height="250" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling="no"&gt;&lt;/iframe&gt;');
    }
});

var hideSub;
$('#navWrapper').bind("mouseleave",function() {
    clearTimeout(openSub);
    if ($(".subNav").css("display") == 'none') {
        $('.main-nav ul li').removeClass('active');
    }
    hideSub=setTimeout("$('#subnavWrapper .subNav').slideUp(); subNavClosed = true; $('.main-nav ul li').removeClass('active');", 1000);
});
$('#navWrapper').bind("mouseenter",function() {
    clearTimeout(hideSub);
});

function openNav(id) {
    if ($('.main-nav ul li').hasClass('active')) {
        $("#subnavWrapper .subNav").hide();
        $("#"+id+"SubNav").slideDown()
        subNavClosed = false;
    }
}

$('.showHeadlines').mouseover(function() {
    if ($("#"+this.id).parent().hasClass("active") == false) {
        if (typeof request != "undefined") {
            request.abort();
        }
        if (currentHeadlinesId != this.id &amp;&amp; $("#"+this.id+"Headlines").html() == "") {
            currentHeadlinesId = this.id;
            request = $.ajax({
                url: "/block?name=subnav-"+currentHeadlinesId
            }).done(function( html ) {
                $("#"+$("#"+currentHeadlinesId).parent().parent().parent()[0].id + " .headlinesHolder").hide();
                $("#"+currentHeadlinesId+"Headlines").html(html).show();
            });
        } else if (currentHeadlinesId != this.id) {
            currentHeadlinesId = this.id;
            $("#"+$("#"+currentHeadlinesId).parent().parent().parent()[0].id + " .headlinesHolder").hide();
            $("#"+currentHeadlinesId+"Headlines").show();
        }
        currentHeadlinesId = this.id;
        $("#"+$("#"+currentHeadlinesId).parent().parent()[0].id+" li").removeClass("active");
        $("#"+currentHeadlinesId).parent().addClass("active");
        adContainer = $("#"+currentHeadlinesId).parent().parent()[0].id.replace("List", "");
        //$("#"+adContainer+"Ad").html('&lt;iframe style="background: #DADADA;" src="http://ad.doubleclick.net/N5578/adi/nav_'+adContainer+';sz=160x300;ord=' + ord + '?" width="160" height="300" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling="no"&gt;&lt;/iframe&gt;');
    }
});

