          <div style="margin-top:-35px" id="fix_marker">
            В 
          </div>
          <script type="text/javascript">
//<![CDATA[

	jQuery.noConflict();
	jQuery(document).ready(function($) {
	
		var _window = $(window);
		var _fix_block = $('#fix_marker');
		_fix_block.parents('.right').attr('id','fix_col');
		var navigation = $('#fix_col');
		var navigation_left = (navigation.offset().left).toFixed(0);

		var bottom = $('.bottom_banner');

		var _fix_block_top;
		var bottom_top;
		var _col_top;
		var _col_height;
		var _position;
		
		var n = 0;
		_window.scroll(function() {
			if (n == 0)
			{
				_fix_block_top = _fix_block.offset().top;
				bottom_top = bottom.offset().top;
				_col_height = bottom_top - $('.content').offset().top;
				n++;
			}
			_col_top = $(document).scrollTop()+_window.height();
			
			//$('#_info').text(n+' '+_col_height+' '+_col_top+' '+_fix_block_top+' '+bottom_top+' '+_col_height+' '+_position);
				
			if (_col_top &lt; _fix_block_top) 
			{
				if (_position != 0)
				{
					navigation.removeAttr('style');
					_position = 0;
				}	
			}
			else 
			{
				if (_col_top &lt; bottom_top)
				{
					if (_position != 1)
					{
						navigation.removeAttr('style').css({
							'position':'fixed',
							'left':navigation_left+'px',
							'bottom':0
						});
						_position = 1;
					}
				}
				else
				{
					if (_position != 2)
					{
						navigation.removeAttr('style').css({
							//'position':'static',
							'margin-top':(_col_height - navigation.height()).toFixed(0)+'px'
						});
						_position = 2;
					}
				}
			}
		});

	});

//]]>
          </script>
