//<![CDATA[


function TMT_onload() {
}

function getXmlHttp(){
  var xmlhttp;
  try {
    xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
  } catch (e) {
    try {
      xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    } catch (E) {
      xmlhttp = false;
    }
  }
  if (!xmlhttp &amp;&amp; typeof XMLHttpRequest!='undefined') {
    xmlhttp = new XMLHttpRequest();
  }
  return xmlhttp;
}

var xmlHttp = getXmlHttp();

function month_change(year) {
  var month = document.getElementById("month_sel").value;
  callServer(month,year);
}

function update_calendar() {
  if (xmlHttp.readyState == 4) {
    var response = xmlHttp.responseText;
    document.getElementById("calendar").innerHTML = response;
  }
}

function callServer(month,year) {
  // РЎРѕР·РґР°С‚СЊ URL РґР»СЏ РїРѕРґРєР»СЋС‡РµРЅРёСЏ
  var url = "/scripts/CalendarUpdate.php?month=" + escape(month) + "&amp;year=" + escape(year);

  // РћС‚РєСЂС‹С‚СЊ СЃРѕРµРґРёРЅРµРЅРёРµ СЃ СЃРµСЂРІРµСЂРѕРј
  xmlHttp.open("GET", url, true);

  // РЈСЃС‚Р°РЅРѕРІРёС‚СЊ С„СѓРЅРєС†РёСЋ РґР»СЏ СЃРµСЂРІРµСЂР°, РєРѕС‚РѕСЂР°СЏ РІС‹РїРѕР»РЅРёС‚СЃСЏ РїРѕСЃР»Рµ РµРіРѕ РѕС‚РІРµС‚Р°
  xmlHttp.onreadystatechange = update_calendar;

  // SРџРµСЂРµРґР°С‚СЊ Р·Р°РїСЂРѕСЃ
  xmlHttp.send(null);
}

function update_photo() {
  if (xmlHttp.readyState == 4) {
    var response = xmlHttp.responseText;
    document.getElementById("article_photos").innerHTML = response;
  }
}

function callPhotoServer(photo, article, lang) {
  // РЎРѕР·РґР°С‚СЊ URL РґР»СЏ РїРѕРґРєР»СЋС‡РµРЅРёСЏ
  var url = "/scripts/PhotoUpdate.php?photo=" + escape(photo) + "&amp;article=" + escape(article) + '&amp;lang=' + escape(lang);

  // РћС‚РєСЂС‹С‚СЊ СЃРѕРµРґРёРЅРµРЅРёРµ СЃ СЃРµСЂРІРµСЂРѕРј
  xmlHttp.open("GET", url, true);

  // РЈСЃС‚Р°РЅРѕРІРёС‚СЊ С„СѓРЅРєС†РёСЋ РґР»СЏ СЃРµСЂРІРµСЂР°, РєРѕС‚РѕСЂР°СЏ РІС‹РїРѕР»РЅРёС‚СЃСЏ РїРѕСЃР»Рµ РµРіРѕ РѕС‚РІРµС‚Р°
  xmlHttp.onreadystatechange = update_photo;

  // SРџРµСЂРµРґР°С‚СЊ Р·Р°РїСЂРѕСЃ
  xmlHttp.send(null);
}

//]]>
