                      <script type="text/javascript">
//<![CDATA[

function change_sections(targetId, id) {
	var Select = document.getElementById(targetId);
	var div = document.getElementById('div_' + id);
	for (var i = 0; i &lt; Select.options.length; i++) {
		if (Select.options[i].value == id) {
			var option = Select.options[i];
		}
	}
	var temp = id.split("_");
	if (div.className == 'iblock') {
		if(option.selected) {
			div.style.backgroundColor = '#ffffff';
			option.selected = false;
			if($(temp[0]+'_sections').getStyle('display') == 'none') {
				Effect.BlindDown(temp[0]+"_sections",{duration: 0.5});
//				document.getElementById(temp[0]+"_sections").style.display="block";
				document.getElementById(temp[0]+'_arrow').innerHTML = '&amp;#9660;';
			}
		} else {
			div.style.backgroundColor = '#e6e6e6';
			option.selected = true;
			var sectionsDiv = document.getElementById(temp[0]+"_sections");
			if($(temp[0]+'_sections').getStyle('display') == 'block') {
				Effect.BlindUp(sectionsDiv,{duration: 0.5});
//				sectionsDiv.style.display="none";
				document.getElementById(temp[0]+'_arrow').innerHTML = '&amp;#9658;';
			}
			var children = sectionsDiv.getElementsByTagName('div');
			for(var i = 0; i &lt; children.length; i++) {
				var child_id = children[i].id;
				child_id = child_id.split("_");
				for (var j = 0; j &lt; Select.options.length; j++) {
					if(Select.options[j].value == child_id[1]+"_"+child_id[2]) {
						Select.options[j].selected = false;
						children[i].style.backgroundColor = '#ffffff';
					}
				}
			}
		}
	} else {
		if(option.selected) {
			div.style.backgroundColor = '#ffffff';
			option.selected = false;
		} else {
			div.style.backgroundColor = '#e6e6e6';
			option.selected = true;
			for (var i = 0; i &lt; Select.options.length; i++) {
				if (Select.options[i].value == temp[0]+'_0') {
					Select.options[i].selected = false;
				}
			}
			document.getElementById('div_'+temp[0]+'_0').style.backgroundColor = '#ffffff';
		}
	}
}
function open_close_section (targetId,id) {
	var Select = document.getElementById(targetId);
	if($(id+'_sections').getStyle('display') == 'block') {
		Effect.BlindUp(id+'_sections',{duration: 0.5});
		document.getElementById(id+'_arrow').innerHTML = '&amp;#9658;';
	} else if($(id+'_sections').getStyle('display') == 'none') {
		for (var i = 0; i &lt; Select.options.length; i++) {
			if (Select.options[i].value == id+'_0') {
				Select.options[i].selected = false;
			}
		}
		document.getElementById('div_'+id+'_0').style.backgroundColor = '#ffffff';
		Effect.BlindDown(id+'_sections',{duration: 0.5});
		document.getElementById(id+'_arrow').innerHTML = '&amp;#9660;';
	}
}
function open_close_all_site (targetId) {
	var Select = document.getElementById(targetId);
	if($('sections_list').getStyle('display') == 'block') {
		Effect.BlindUp('sections_list',{duration: 0.5});
		document.getElementById('all_site_arrow').innerHTML = '&amp;#9658;';
	} else if($('sections_list').getStyle('display') == 'none') {
		for (var i = 0; i &lt; Select.options.length; i++) {
			if (Select.options[i].value == '0') {
				Select.options[i].selected = false;
			}
		}
		document.getElementById('div_all_site').style.backgroundColor = '#ffffff';
		Effect.BlindDown('sections_list',{duration: 0.5});
		document.getElementById('all_site_arrow').innerHTML = '&amp;#9660;';
	}
}
function change_sections_all_site (targetId) {
	var Select = document.getElementById(targetId);
	for (var i = 0; i &lt; Select.options.length; i++) {
		if (Select.options[i].value == '0') {
			var option = Select.options[i];
		}
	}
	if(option.selected) {
		option.selected = false;
		Effect.BlindDown('sections_list',{duration: 0.5});
		document.getElementById('all_site_arrow').innerHTML = '&amp;#9660;';
		document.getElementById('div_all_site').style.backgroundColor = '#ffffff';
	} else {
		for (var i = 0; i &lt; Select.options.length; i++) {
			if (Select.options[i].value != option.value) {
				Select.options[i].selected = false;
				var temp = Select.options[i].value;
				document.getElementById('div_' + temp).style.backgroundColor = "#ffffff";
			}
		}
		option.selected = true;
		Effect.BlindUp('sections_list',{duration: 0.5});
		document.getElementById('all_site_arrow').innerHTML = '&amp;#9658;';
		document.getElementById('div_all_site').style.backgroundColor = '#e6e6e6';
	}
}

//]]>
                      </script>
