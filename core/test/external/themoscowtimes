    <script language="Javascript" type="text/javascript">
//<![CDATA[

function showPopup (text, close_text, width) {
  if (close_text != null &amp;&amp; close_text != '') {
    document.getElementById('popup_close').value = close_text;
  }
  var popup = document.getElementById('popup');
  var popup_text = document.getElementById('popup_text');
  popup_text.innerHTML = text;
  popup.style.display = 'block';
  if (!width) {
    var width = popup.offsetWidth;
  } else {
    popup.style.width = width + 'px';
    width = width + 60;
  }
  var height = popup.offsetHeight;
  popup.style.display = 'none';
  showLayer ('popup', width, height);
}

function closePopup () {
  document.getElementById('popup_close').value = 'Close';
  document.getElementById('popup_text').innerHTML = '';
  hideLayer ('popup');
}

//]]>
    </script>
