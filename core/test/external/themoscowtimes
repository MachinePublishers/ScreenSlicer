.iblock {width:150px;margin:1px 0 1px 31px;color:#55f;cursor:pointer;-moz-border-radius: 10px;-webkit-border-radius: 10px;padding:1px 7px;}
.sections {width:150px;margin:0 0 1em 43px;}
