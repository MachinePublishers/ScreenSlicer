//<![CDATA[

function showTMTtoolbarAd () {
  document.getElementById('TMTtoolbarAd').style.display = 'none';
  if (navigator.userAgent.match(/(firefox)|(safari)/i) &amp;&amp; !navigator.userAgent.match(/(android)|(ios)|(mobile)/i) &amp;&amp; !navigator.userAgent.match(/chrome/i))
    document.getElementById('TMTtoolbarAd').style.display = 'block';
}
function setCookie4TMTtoolbarAd (typeId) {
  var days = 3;
  var date = new Date();
  date.setTime (date.getTime () + (days*24*60*60*1000)); // days*hours*minutes*seconds*milliseconds
  document.cookie = 'TMTtoolbarAd=' + typeId + '; expires=' + date.toGMTString() + '; path=/';
  if (typeId == 1)
    window.location.href = 'http://www.themoscowtimes.com/rss/toolbars/index.php';
}

//]]>
    </script>
