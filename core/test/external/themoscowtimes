              <script type="text/javascript">
//<![CDATA[

function classifieds2_block_change (MenuItem) {
  if(MenuItem == 1)
  {
    document.getElementById('clas2').style.display = 'block';
    document.getElementById('re2').style.display = 'none';
    document.getElementById('mg2').style.display = 'none';
    document.getElementById('clas_head2').style.display = 'block';
    document.getElementById('re_head2').style.display = 'none';
    document.getElementById('mg_head2').style.display = 'none';
  }
  if(MenuItem == 2)
  {
    document.getElementById('clas2').style.display = 'none';
    document.getElementById('re2').style.display = 'block';
    document.getElementById('mg2').style.display = 'none';
    document.getElementById('clas_head2').style.display = 'none';
    document.getElementById('re_head2').style.display = 'block';
    document.getElementById('mg_head2').style.display = 'none';
  }
  if(MenuItem == 3)
  {
    document.getElementById('clas2').style.display = 'none';
    document.getElementById('re2').style.display = 'none';
    document.getElementById('mg2').style.display = 'block';
    document.getElementById('clas_head2').style.display = 'none';
    document.getElementById('re_head2').style.display = 'none';
    document.getElementById('mg_head2').style.display = 'block';
  }
}

//]]>
              </script>
            </div>
