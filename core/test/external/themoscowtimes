              <script type="text/javascript">
//<![CDATA[

function swap_today(targetId,event) {
if (!event) var event=window.event;
    if (document.getElementById){
        var usedids = new Array ('4818','2256','1107','2086','9477');
        var target = document.getElementById('today_article_'+targetId);
        for(j=0; j &lt; usedids.length; j++) {
            target = document.getElementById('today_article_'+usedids[j]);
            if(usedids[j] != targetId) {
                target.style.display = 'none';
            } else {
                target.style.display = 'block';
            }
        }
    }
}

//]]>
              </script>
