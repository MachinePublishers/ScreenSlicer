//<![CDATA[

function swap_month(targetId,event) {
if (!event) var event=window.event;
    if (document.getElementById){
        var usedids = new Array ('5756','6954','1232','6178','1823');
        var target = document.getElementById('month_article_'+targetId);
        for(j=0; j &lt; usedids.length; j++) {
            target = document.getElementById('month_article_'+usedids[j]);
            if(usedids[j] != targetId) {
                target.style.display = 'none';
            } else {
                target.style.display = 'block';
            }
        }
    }
}

//]]>
              </script>
