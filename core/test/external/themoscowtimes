            <script type="text/javascript">
//<![CDATA[

function most_read_change (MenuItem) {
  if(MenuItem == 1)
  {
    document.getElementById('today_head').style.display = 'block';
    document.getElementById('week_head').style.display = 'none';
    document.getElementById('month_head').style.display = 'none';
    document.getElementById('most_popular_today').style.display = 'block';
    document.getElementById('most_popular_week').style.display = 'none';
    document.getElementById('most_popular_month').style.display = 'none';
  }
  if(MenuItem == 2)
  {
    document.getElementById('today_head').style.display = 'none';
    document.getElementById('week_head').style.display = 'block';
    document.getElementById('month_head').style.display = 'none';
    document.getElementById('most_popular_today').style.display = 'none';
    document.getElementById('most_popular_week').style.display = 'block';
    document.getElementById('most_popular_month').style.display = 'none';
  }
  if(MenuItem == 3)
  {
    document.getElementById('today_head').style.display = 'none';
    document.getElementById('week_head').style.display = 'none';
    document.getElementById('month_head').style.display = 'block';
    document.getElementById('most_popular_today').style.display = 'none';
    document.getElementById('most_popular_week').style.display = 'none';
    document.getElementById('most_popular_month').style.display = 'block';
  }
}

//]]>
            </script>
