//<![CDATA[

&lt;!--
	$(document).ready(function() {
		// do stuff when DOM is ready
		if (isAdvancedSearch()) {
			$("#advSrch").show();
		} else {
			$("#advSrch").hide();
		}
		$("#advSrchLink").click(function () {
			if ($("#advSrch").is(":hidden")) {
				$("#advSrch").animate({ height: 'show', opacity: 'show' }, 'medium');
				$("#advSrchLink").html("&amp;laquo; Advanced Search");
			} else {
				$("#advSrch").animate({ height: 'hide', opacity: 'hide' }, 'medium');
				$("#advSrchLink").html("Advanced Search &amp;raquo;");
			}
		});
		function show(id) {
			if ($(id).is(":hidden")) {
				$(id).animate({ height: 'show', opacity: 'show' }, 'medium');
			}
		}

		function hide(id) {
			if (!$(id).is(":hidden")) {
				$(id).animate({ height: 'hide', opacity: 'hide' }, 'fast');
			}
		}
	});
	
	function isAdvancedSearch() {
		var from = document.getElementById("from_date").value;
		var to = document.getElementById("to_date").value;
		var author = document.getElementById("author").value;
		var issue = document.getElementById("issuenumber").value;
		
		if (from=="" &amp;&amp; to=="" &amp;&amp; author=="" &amp;&amp; issue=="") {
			return false;
		} else {
			return true;
		}
	}
	
//--&gt;

