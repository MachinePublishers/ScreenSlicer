    <script language="javascript" type="text/javascript">
//<![CDATA[

&lt;!--
	$(document).ready(function() {
		// do stuff when DOM is ready
		$("#refineby_0_link").click(function () { toggleNavLink("0"); });
		$("#refineby_0_more_link").click( function () { toggleMoreLink("0"); });
		$("#refineby_0_close_link").click(function () { toggleNavLink("0"); });
		
		function toggleNavLink(id) {
			hide("#refineby_"+id+"_more_div");
			toggle("#refineby_"+id+"_div");
		}
		
		function toggleMoreLink(id) {
			toggle("#refineby_"+id+"_more_div");
			/*
			if (!$("#refineby_"+id+"_more_div").is(":hidden")) {
				$("#refineby_"+id+"_more_link").html("more &amp;raquo;");
			} else {
				$("#refineby_"+id+"_more_link").html("&amp;laquo; less");
			}
			*/
		}
		
		function toggle(id) {
			if ($(id).is(":hidden")) {
				show(id);
			} else {
				hide(id);
			}
		}

		function show(id) {
			if ($(id).is(":hidden")) {
				$(id).animate({ height: 'show', opacity: 'show' }, 'medium');
			}
		}

		function hide(id) {
			if (!$(id).is(":hidden")) {
				$(id).animate({ height: 'hide', opacity: 'hide' }, 'fast');
			}
		}
		
		});
// --&gt;

