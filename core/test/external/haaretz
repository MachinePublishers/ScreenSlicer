    <script language="javascript" type="text/javascript">
//<![CDATA[

  
function isLegalReferrer ()
  {
    if ((document.referrer.indexOf("twitter.com") &gt; -1) ||
        (document.referrer.indexOf("facebook.com") &gt; -1) ||
        (document.referrer.indexOf("google.com") &gt; -1) ||
        (document.referrer.indexOf("google.co.il") &gt; -1) ||
      (document.referrer.indexOf("news.google.com") &gt; -1) ||
        (document.referrer.indexOf("t.co") &gt; -1) ||
      (document.referrer.indexOf("plus.google.com") &gt; -1))
       {
          return true;
      } else
      {
        return false;
      }
    
  }

var userAgent = navigator.userAgent.toLowerCase();
var isIphone = userAgent.indexOf("iphone") &gt; -1;
var isAndroid = userAgent.indexOf("android") &gt; -1;
var isBlackberry = userAgent.indexOf("blackberry") &gt; -1;

var encryptedId = '';
var contentId='VersionedContentId(7.964334.1381741347)'; 
var originalContentId='[ContentId(2.169), ContentId(2.246), ContentId(2.250)]'; 
  // +"&amp;contentId="+contentId+"&amp;original="+originalContentId

if ((isIphone || isAndroid || isBlackberry) &amp;&amp; document.cookie.indexOf("doNotRedirectToMobile=") &lt; 0)
{
if(typeof isArticle != 'undefined' &amp;&amp; isArticle &amp;&amp; typeof articleId != 'undefined')
{ 
  if (isLegalReferrer ()){
    document.location.href = "/mobile/" + articleId +'?v='+encryptedId;

  } else{
    document.location.href = "/mobile/" + articleId;
  }
}
else
{ 
  document.location.href = "/mobile"; }
}


//]]>
    </script>
