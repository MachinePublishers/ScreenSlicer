    <script>
//<![CDATA[

var SiteCatalyst = {};
SiteCatalyst.reportSuite = 'reportSuite';
SiteCatalyst.registerEvent = function(eventDetails){
    //in case the s_code library was not included
    if (s === undefined)
        return;
    s=s_gi(s_account);
    var linkTrackVars = [];
    if (eventDetails.events !== undefined &amp;&amp; eventDetails.events.length &gt; 0)
    {
        s.linkTrackEvents = eventDetails.events;
        s.events = eventDetails.events;
        linkTrackVars.push("events");
    }
    if (eventDetails.eVariables !== undefined &amp;&amp; eventDetails.eVariables.length &gt; 0)
    {
        var evars = eventDetails.eVariables.split(",");
    	for (var count = 0; count &lt; evars.length; count++) {
        	linkTrackVars.push(evars[count]);
        	// if eVal is defined use it to populate the properties to be sent along with the custom link
        	if (eventDetails.eValues !== undefined &amp;&amp; eventDetails.eValues.length &gt; 0) {
        		s[evars[count]] = eventDetails.eValues[count];
        	}
        	else {
        		s[evars[count]] = eventDetails.title;
        	}			
    	}
    }
    s.linkTrackVars = linkTrackVars.join();
    s.tl(this,'o',eventDetails.title);
	// clear variables to not have the event recorded again [along with l_e(xit)]
	s.linkTrackVars="None";
	s.linkTrackEvents="None";
	s.events='';
    return;
};

//]]>
    </script>
