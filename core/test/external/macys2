              <script type="text/javascript">
//<![CDATA[

(function() {
//search query
var query = null;
//adpage is for ad pagination and default value is 1
var adpage = 1;
//facets for search
var queryContext = null;
//publisher Id for the browse pages 
var pubId = 'macys';
var pageIndex = null, loc_href = window.location.href;
//adtest must be set to OFF on Production Environment 
adtest = ("true" == "true") ? "off":"on";
function getHashBangUrl() {
var sheBangUrl = null;
var sheBangStartIdx = loc_href.indexOf('#!');
if(sheBangStartIdx &gt; -1) {
sheBangUrl = loc_href.substr(sheBangStartIdx+2);
}
return sheBangUrl;
}
function getPageIndex() {
var sheBangUrl = getHashBangUrl();
var patt=/pageindex=\d/;
var index=1;
var result=null;
if(sheBangUrl === null)
return index;
sheBangUrl = decodeURIComponent(sheBangUrl.toLowerCase());
if(sheBangUrl.indexOf('pageindex=') &gt; -1) {
result=patt.exec(sheBangUrl);
index = (result === null) ? 1 : result[0].split('=')[1]; 
}
return index;
}
function getSearchQueryContext() {
var facets = '';
var divider = ' ';
var facetCtx = '';
var sheBangUrl = getHashBangUrl();
if(sheBangUrl === null)
return null;
sheBangUrl = decodeURIComponent(sheBangUrl.toLowerCase());
sheBangUrl = sheBangUrl.replace(/pageindex=\d/,'');
var sidx = sheBangUrl.indexOf("fn=");
var eidx = sheBangUrl.indexOf("&amp;sortby=");
if (sidx === -1 || eidx === -1)
return null;
if (eidx &gt; sidx) {
facets = decodeURIComponent(sheBangUrl.substring(sidx+3, eidx));
var fsArr = facets.split('&amp;');
for (i = 0, len = fsArr.length; i &lt; len; i++) {
facetCtx += decodeURIComponent(fsArr[i].replace(/\s/g,'+')) + divider;
}
}
return facetCtx.substring(0, facetCtx.length);
}
function getParam(name) {
return YAHOO.util.History.getQueryStringParameter(name);
}
function getText(elem) {
if (elem.textContent) {
elem.textContent = changeVal;
} else {
elem.innerText = changeVal;
}
}
//getting the Searched Keyword 
query = getParam('keyword');
//get page index from URL
pageIndex = getPageIndex();
if (query === null) {
// if query is null get it from the search text 
try {
query = YAHOO.util.Dom.getElementsByClassName("keyWord", "h1",
"searchResultIntroTextLeftCol")[0].innerHTML.replace(
/&amp;amp;/g, "&amp;");
} catch (e) {}
}
//set channel based on the URL
if (loc_href.indexOf("/shop/") != -1 || loc_href.indexOf("/buy/") != -1) {
if("" === "wedding") {
channel = 'bridalsearch';
} else {
channel = 'sitesearch';
}
}
//refinements will be captured from the facet selection that user makes
if (query != null) {
//get facets
queryContext = getSearchQueryContext();
}
queryContext = (queryContext === null) ? '':' '+queryContext;
if (query != null) {
var pageOptions = {
pubId : pubId,
query : query+queryContext,
queryLink : '',
queryContext : queryContext,
channel : channel,
adtest : adtest,
adpage : adpage,
sellerRatings : false,
siteLinks : true,
ie : 'ISO-8859-1',
oe : 'ISO-8859-1'
};
var footerAds = {
container : 'googleAdsense',
number : '6',
fontFamily : 'verdana,arial,lucida,sans-serif',
fontSizeTitle : '11px',
fontSizeDescription : '11px',
fontSizeDomainLink : '11px',
colorTitleLink : '#000000',
colorText : '#000000',
colorDomainLink : '#999999',
linkTarget : '_blank'
};
new google.ads.search.Ads(pageOptions, footerAds);
} else {
document.getElementById("googleAdsense").style.display = "none";
}
})();

//]]>
              </script>
