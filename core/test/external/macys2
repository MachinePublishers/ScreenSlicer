    <script type="text/javascript">
//<![CDATA[

MACYS.namespace("SearchAnalytics");
MACYS.SearchAnalytics.InfoDLP = (function(href, previousHref) {
var regexDLP = /^(\S+)[/]((buy)|(shop[/]featured))[/](\S+)$/,
regexWedding = {
currentPage: /^(\S+)[?&amp;]mode=wedding([#&amp;]\S+)?$/,
previousPage: /^(\S+)[/]wedding-registry[/](\S+)$/
},
regexShopByBrand = /^(\S+)[?&amp;]cm_sp=shop_by_brand(\S+)$/,
isDLP = regexDLP.test(href),
isPreviousPageWedding = regexWedding.previousPage.test(previousHref),
isCurrentPageWedding = regexWedding.currentPage.test(href),
isModeWedding = isCurrentPageWedding || isPreviousPageWedding,
isShopByBrand = regexShopByBrand.test(href),
info = {
isDLP: isDLP,
isDLPModeWedding: isDLP &amp;&amp; isModeWedding,
isDLPShopByBrand: isDLP &amp;&amp; isShopByBrand,
trackWeddingPageIfNeed: function(tracking) {
var haveToTrack = !isCurrentPageWedding &amp;&amp; isPreviousPageWedding;
if (haveToTrack &amp;&amp; tracking) {
tracking();
}
}
};
if (!isDLP) return info;
if (isShopByBrand) {
info.categoryID = isModeWedding ? "MWEDD_Dynamic_Landing_Pages_Brand" : "Dynamic_Landing_Pages_Brand";
} else {
info.categoryID = isModeWedding ? "MWEDD_Dynamic_Landing_Pages_LM" : "Dynamic_Landing_Pages_LM";
}
return info;
}(location.href, document.referrer));
MACYS.SearchAnalytics.updatePageViewTag = function() {
var pageID = "search_results";
var categoryID = "onsite_search";
var searchKeyword = YAHOO.util.Dom.get("keywordSearch");
var autocompleteKeyword = MACYS.util.Url.getQueryStringParameter("cm_kws_ac");
var searchResults = "2686";
var countryName = MACYS.util.Cookie.get("shippingCountry");
var rewrittenPhrase = YAHOO.util.Dom.get("rewrittenSearchPhrase");
var exploreAttr = new exploreAttributes();
var infoDLP = MACYS.SearchAnalytics.InfoDLP;
if(searchKeyword) {
// TODO: Hash logic would be moved to different approach in later release.
if(!(window.location.hash || infoDLP.isDLP)) {
searchKeyword = searchKeyword.value;
window.location.hash = "cm_pv=slp";
} else {
searchKeyword = "";
}
}
//New Search Action
if(rewrittenPhrase) {
pageID = rewrittenPhrase.value;
categoryID = "new_search";
}
if("" == "wedding") {
categoryID = "MWEDD_" + categoryID;
}
if(countryName &amp;&amp; countryName != 'US'){
countryName = "International_" + countryName;
}else {
countryName = 1;
}
exploreAttr.add({
15: countryName
});
if(autocompleteKeyword) {
categoryID += "_autocomplete";
exploreAttr.add({
22: unescape(autocompleteKeyword)
});
}
categoryID = infoDLP.isDLP ? infoDLP.categoryID : categoryID;
MACYS.coremetrics.cmCreatePageviewTag(pageID, categoryID, searchKeyword, searchResults, exploreAttr.toString());
};
MACYS.SearchAnalytics.updatePageViewTag();
MACYS.killswitchImgSprite = true;

//]]>
    </script>
    <!-- Check for registry mode start-->    <!-- Check for registry mode end -->    <!-- Check for fobCategoryID,Name and topLevelCategoryID,Name start -->    <!-- Check for fobCategoryID,Name and topLevelCategoryID,Name end -->    <script type="text/javascript">
//<![CDATA[

MACYS.namespace("brightTag");
var navigation={
"pageName" : "",
"pageID" : "",
"registryMode" : "NO"
};
var category={
"categoryID" : "",
"categoryName" : "",
"topLevelCategoryID" : "",
"topLevelCategoryName" : "",
"fobCategoryID" : "",
"fobCategoryName" : ""
};
MACYS.brightTag ={"navigation": navigation,"category": category};

//]]>
    </script>
