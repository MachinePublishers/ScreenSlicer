    safeCallback = function (loaded, callback) {
      return function (url, result, key) {
        if (!loaded()) {
          try {
            Airbrake.notify({
              message: "ConditionalAssets: " + key + " failed to load"
            });
          } catch (error) { }

          return;
        }

        callback(url, result, key);
      }
