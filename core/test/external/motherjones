//<![CDATA[

  twttr.ready(function (twttr) {
    twttr.events.bind('follow',
        function (intent_event) {
            _gaq.push(['_trackEvent', 'Right Hand Column', 'Right Hand Column Twitter Follow', window.location.pathname]);
        })
    twttr.events.bind('tweet',
        function (intent_event) {
            if (intent_event) {
              var clicklocation;
              if (jQuery(intent_event.target).parent().parent().parent().parent().attr('id') === 'node-header') {
                    clicklocation = 'Content Top';
              } else if (jQuery(intent_event.target).parent().parent().parent().attr('id') === 'node-footer') {
                    clicklocation = 'Content Bottom';
              } else {
                   console.log('could not determine which like was clicked, event came from ')
                   console.log(intent_event);
                   return;
              }
              _gaq.push(['_trackEvent', clicklocation, clicklocation + ' Twitter Tweet', window.location.pathname]);
           }
        }
    )
  });
FB.Event.subscribe('edge.create', function(targetUrl, widget) {
console.log('fb liking');
    var clicklocation;
    var action_type;
      if (jQuery(widget).parent().parent().parent().parent().parent().parent().parent().attr('id') === "sidebar-right-inner") {
            clicklocation = 'Right Hand Column'
            action_type = 'Right Hand Column Facebook Follow';
      } else if (jQuery(widget).parent().parent().parent().parent().attr('id') === 'node-header') {
            clicklocation = 'Content Top';
            action_type = 'Content Top Facebook Like';
      } else if (jQuery(widget).parent().parent().parent().attr('id') === 'node-footer') {
            clicklocation = 'Content Bottom';
            action_type = 'Content Bottom Facebook Like';
      } else {
           console.log('could not determine which like was clicked, event came from ')
           console.log(widget);
           return;
      }
        _gaq.push(['_trackEvent', clicklocation, action_type, window.location.pathname]);
});

//]]>
                  </script>
