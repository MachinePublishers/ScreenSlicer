          <script type="text/javascript">
//<![CDATA[

  $(document).ready(function () {
    $("#poll_7_275175").attr("target", "pollResultWindow");
    
    $("div.pollColumn").each(function(i) {
      var divId = $(this).parent("div").attr("id");

      if(divId == "col1") {
      	$(this).css("width", "97.7%");
      }
    });
  });
  
  getDivWidth = function() {
    return $('div.pollColumn').width() + 100;
  }
  
  getDivHeight = function() {
    return $('div.pollColumn').height() + 66;
  }

  function checkPreSubmit() {
    var formElement = document.getElementById("poll_7_275175");
    var radioElement = document.getElementsByName("answerId");
    var selected = -1;
    
    for(var i = 0; i &lt; radioElement.length; i++) {
      if(radioElement[i].checked) {
        selected = i;
        break;
      }
    }
    
    if(selected == -1) {
      alert("Please make your selection!");
      return false;
    }
	
    document.forms["poll_7_275175"].submit();

    return true;
  }

//]]>
          </script>
