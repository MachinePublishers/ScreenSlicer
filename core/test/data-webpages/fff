//<![CDATA[

  var sharetopoffset = jQuery("#supportlink").offset().top - parseFloat(jQuery("#supportlink").css("margin-top").replace(/auto/,0));
  
   jQuery(window).scroll(function(event) {
       var y = jQuery(this).scrollTop();
       if(y &gt;= sharetopoffset) {
           jQuery("#supportlink").addClass("fixed");
       } else {
           jQuery("#supportlink").removeClass("fixed");
       }
   });

