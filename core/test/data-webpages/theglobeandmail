s.eVar62 = "unlimited upsell complete without paywall";
s.eVar63 = s.eVar62;
}
var completeUpsell = omniSubscribeSuccess();
if (completeUpsell != 'NaN') {
s.eVar67 = completeUpsell;
util.storeIndexValue(tlu.name,3, 'na','|');
}
}
//clearLinkVars();
break;
case 5:
s.events = "event49";
s.linkTrackEvents = s.events;
s.tl(this, 'o', 'Email Verification');
//clearLinkVars();
break;
case 7:
s.events = "event51";
s.linkTrackEvents = s.events;
s.linkTrackVars = 'eVar21';
s.eVar21 = msg;
s.tl(this, 'o', 'Error');
//clearLinkVars();
break;
case 8:
s.events = "event52";
s.linkTrackEvents = s.events;
s.tl(this, 'o', 'Tutorial video play');
//clearLinkVars();
break;
//default:
}
}
