//<![CDATA[

function searchDuplicateLink(startnum, endnum, dup_id)
{
document.searchForm.search_startnum.value = startnum;
document.searchForm.search_endnum.value = endnum;
document.searchForm.search_dupid.value = dup_id;
document.searchForm.submit();
}

//]]>
        </script>
