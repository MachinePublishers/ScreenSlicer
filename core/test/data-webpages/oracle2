//<![CDATA[

jQuery.noConflict();
jQuery(document).ready(function($){
$("form#searchForm input[type=text]").focus();
$("form#searchForm input#q").autocomplete({
source: function( request, response ) {
var formGroup = document.searchForm.group.value
var sg_name = formGroup == null ? '' : formGroup;
$.ajax({
url: "AutoSuggestQuery",
dataType: "json",
data: {
key: request.term,
lang: "en",
sg: "All"
},
success: function( data ) {
response( $.map( data.suggests, function( item ) {
return {
label: item,
value: item
}
}));
}
});
},
minLength: 2,
autoFocus: false,
select: function(event, ui) {
$("form#searchForm input#q").val(ui.item.value);
$("form#searchForm").submit();
}
});
});

//]]>
        </script>
        <script type="text/javascript">
//<![CDATA[

// function toggleAttributeFilters(groupBoxes)
// Delete attr filter rows in case search includes groups other than
// OpenWorld and JavaOne since filters only apply to these sources
function toggleAttributeFilters(groupBoxes) {
var showFilters = true;
var newContainsAll = false;
var oldContainsAll = false;
var selGroupCount = 0;
var newGroupsString = "";
if (groupBoxes[0]) {
for (var i=0; i&lt;groupBoxes.length; i++) {
if (groupBoxes[i].checked) {
selGroupValue = groupBoxes[i].value;
if (selGroupValue == "All") {
newContainsAll = true;
}
selGroupCount++;
}
}
var oldGroupsString = document.forms["selGroupsForm"].elements["listOldSelection"].value;
var oldGroupsArr = oldGroupsString.split(", ");
for (x in oldGroupsArr)
{
if (oldGroupsArr[x] == "All")
{
oldContainsAll = true;
}
}
if ((newContainsAll == true) &amp;&amp; (selGroupCount &gt; 1)) {
if (oldContainsAll == true) {
// Uncheck "All". Something other than "All" was just checked
for (var i=0; i&lt;groupBoxes.length; i++) {
if ((groupBoxes[i].checked) &amp;&amp; (groupBoxes[i].value == "All")) {
groupBoxes[i].checked = false;
}
}
} else {
// Uncheck everything other than "All" since "All" was just checked
for (var i=0; i&lt;groupBoxes.length; i++) {
if ((groupBoxes[i].checked) &amp;&amp; (groupBoxes[i].value != "All")) {
groupBoxes[i].checked = false;
}
}
}
}
selGroupCount = 0;
for (var i=0; i&lt;groupBoxes.length; i++) {
if (groupBoxes[i].checked) {
selGroupValue = groupBoxes[i].value;
if (selGroupValue != "Java.com") {
showFilters = false;
}
if (selGroupCount == 0) {
newGroupsString = selGroupValue;
} else {
newGroupsString = newGroupsString + ", " + selGroupValue;
}
selGroupCount++;
}
}
document.forms["selGroupsForm"].elements["listOldSelection"].value = newGroupsString;
}
//to do: attr filters
//if new list has only JavaOne and Openworld then show the attr filters
// If we can't show filters ...
if (showFilters == false) {
checkAttributeFilters();
}
}
function checkForm(thisForm) {
var selGroupCount = 0;
var showFilters = true;
var groupBoxes = document.searchForm.group;
/* If no source groups are selected, select "All" */
/* See if we need to remove attribute filters */
for (var i=0; i&lt;groupBoxes.length; i++) {
if (groupBoxes[i].checked) {
selGroupValue = groupBoxes[i].value;
if (selGroupValue != "Java.com") {
showFilters = false;
}
if(thisForm.name != 'searchForm')
{
addGroups(groupBoxes[i].value);
}
selGroupCount++;
}
}
if (selGroupCount == 0) {
for (var i=0; i&lt;groupBoxes.length; i++) {
if (groupBoxes[i].value == "All") {
groupBoxes[i].checked = true;
}
}
}
if (showFilters == false) {
checkAttributeFilters();
}
thisForm.submit();
}
function addGroups(selGroupValue)
{
new_element = document.createElement("input");
new_element.setAttribute("type","hidden");
new_element.setAttribute("name", "group");
new_element.setAttribute("id", "");
new_element.setAttribute("value", selGroupValue);
document.forms['searchForm_bottom'].appendChild(new_element);
}
//addGroup() closed
function checkAttributeFilters() {
var numFilters = ses.InlineAdvancedView.countValidAttrFilters();
if( numFilters &gt; 0 ) {
var lastRow = ses.InlineAdvancedView.getLastRowEl();
if( lastRow ) {
var lastRowId = parseInt( lastRow.getAttribute("rowNum"), 10 );
for (var i=lastRowId; i&gt;0; i--) {
ses.InlineAdvancedView.removeAdvSearchAttr(i);
}
}
}
if( ses.InlineAdvancedView.isInlineAdvSearchPanelVisible() ) {
ses.InlineAdvancedView.toggleInlineAdvSearch();
}
}
function callCheckForm(e,thisForm) {
var code;
if (!e) {
var e = window.event;
}
if (e.keyCode) {
code = e.keyCode;
}
else if (e.which)
{
code = e.which;
}
//alert('keyCode ' + e.keyCode);
if (e.keyCode == 13) {
checkForm(thisForm);
}
}

//]]>
        </script>
