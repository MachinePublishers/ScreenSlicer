  if (getURLByParameter('destination') == 'disqus/closewindow' || getURLByParameter('disqus') == '1') {
    document.write('&lt;link type="text/css" rel="stylesheet" media="all" href="http://www.thedailymeal.com/sites/all/themes/thedailymeal/css/disqus-auth.css" /&gt;');
