function siteSearchSubmit() {
        search_val = document.getElementById("edit-search-theme-form-1").value;

        if (search_val.replace(/^\s*(\S*(?:\s+\S+)*)\s*$/, "$1") != "") {
          // location = "/search/luceneapi_node/" + encodeURIComponent(search_val);
          location = "/search/apachesolr_search/" + encodeURIComponent(search_val);
        }
      }
