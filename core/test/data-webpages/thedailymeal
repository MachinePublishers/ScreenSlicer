    <script language="javascript">
//<![CDATA[

  function getCookie(name) {
    name += '=';
    var parts = document.cookie.split(/;\s*/);
    for (var i = 0; i &lt; parts.length; i++) {
      var part = parts[i];
      if (part.indexOf(name) == 0)
        return part.substring(name.length)
    }
    return null;
  }

  // initial call - set up sitepage and sponsor index

  function adTargetSitePage(PHP_sitepage) {
    targets = PHP_sitepage.split("|");
    if (targets.length&gt;1) {
      var d = new Date();
      var n = d.getMilliseconds();
      ad_target_index = n%targets.length;
      return targets[ad_target_index];
    } else {
      return PHP_sitepage;
    }
  }

  // subsquent placement calls (sponsor logo, more on box, promo box)

  function adTargetSponsorAssets(position) {
    switch(position) {
      case "sponsor-logo-right": html = sponsor_logo_rights; break;
      case "sponsor-logo-content": html = sponsor_logo_contents; break;
      case "sponsor-more-on-box": html = sponsor_more_on_boxes; break;
      case "sponsor-promo-box": html = sponsor_promo_boxes; break;
      default: html = ""; break;
    }
    targets = html.split("|");
    if (targets.length&gt;1) {
      return targets[ad_target_index];
    } else {
      return html;
    }
  }

  var ad_target_sitepage = adTargetSitePage('tdm//search/apachesolr_search/us');

  ad_target_sitepage = ad_target_sitepage.replace("//","/");

  if(getCookie("ac")!="y" || getCookie("as")=="n") ad_target_sitepage = ad_target_sitepage.replace("tdm","tdm/campaign");



//]]>
    </script>
