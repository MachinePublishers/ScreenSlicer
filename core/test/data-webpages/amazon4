var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';

                

  var generateAjaxCall = function(keyword, searchAlias, browseNode, seenAsins, 
    isBrowse, paDebug, pageType, subPageType, placement, useAui, bl, childBrowseNodes) {
    var lazyLoadURL = '/gp/product-ads/lazyLoad/handler/click-within-handler.html';

    jQuery.ajax({
      
      url: lazyLoadURL + "?searchTerms="+encodeURIComponent(keyword)+
                         "&amp;searchAlias="+encodeURIComponent(searchAlias)+
                         "&amp;isDebug=" + paDebug + "&amp;widgetName="+encodeURIComponent(widgetName)+
                         "&amp;pageType=" + pageType + "&amp;subPageType=" + subPageType +
                         "&amp;browseLadder="+encodeURIComponent(bl)+
                         "&amp;childBrowseNodes="+encodeURIComponent(childBrowseNodes)+
                         "&amp;placement=" + placement + "&amp;seenasins="+encodeURIComponent(seenAsins)+
                         "&amp;useAui=" + useAui +
                         "&amp;suggestedBN="+encodeURIComponent(browseNode)+"&amp;isBrowse="+isBrowse,
      type: "GET",
      cache : false,
      success : function(html) { 
        jQuery('#click_withinLazyLoad_' + placement).html(html);    
      },
      error : function(html) { 
        jQuery('#click_withinLazyLoad_' + placement).html("&lt;div&gt;&lt;/div&gt;");    
      }
    }); 
  }


                
  var cw_extractParams = function(inputString) {
    var paramString = unescape(inputString); 
    var eParams = paramString.split(",");
    return eParams; 
  } 

  var regexRh =  /([?&amp;=])(rh=)([^&amp;]*)/;

  var shouldPunt = function(bl) {
    var cwurl = location.href;
    var regexField = /([?&amp;=])(field-brandtextbin=)([^&amp;]*)/;                  
    var regexSort = /([?&amp;=])(sort=)([-\w]+)/; 
    var regexMe = /([?&amp;=])(me=)([^&amp;]*)/; 
    var regexScn = /([?&amp;=])(scn=)([-\w]+)/; 
    var regexSrs = /(srs=)([^&amp;]*)/;
    var regexSearchAlias = /(search-alias=)([^&amp;]*)/;
    var hashindex = cwurl.indexOf("#"); 
    var rhstring;
    var sortstring;

    if (hashindex == -1) {
      rhstring = regexRh.exec(cwurl);
      sortstring = regexSort.exec(cwurl);
    } else {   
      hashstring = cwurl.substring(hashindex+1);
      rhstring = regexRh.exec(hashstring);
      sortstring = regexSort.exec(hashstring);
    }

    if (bl == null || bl.length == 0) {
      return true;             
    }

    if (rhstring != null &amp;&amp; rhstring.length != 0) {
      var rhParams = cw_extractParams(rhstring);       
      var pat = /(\b[n|k|i]\b)/; 
      for (var x = 0; x &lt; rhParams.length; x++) {
        var entry, rhKey;
        entry = rhParams[x].split(":"); 
        if (entry.length == 2) {
          rhKey = entry[0];                 
          if(!pat.test(rhKey)) {                          
            return true;
          }
        } 
      }
    }
    if (regexField.test(unescape(cwurl))) {                     
      return true;
    }
    if(regexMe.test(unescape(cwurl))) {                    
      return true;
    }

    var pantryPunt = false;

    var areParamAndValuePresent = function(params, name, value) {
      if (params) {
        for(var i = 0; i &lt; params.length; ++i) {
          var splitParams = params[i].split('=');
          if (splitParams.length &gt; 1 &amp;&amp; splitParams[0] === name) {
            if (splitParams[1] === value) {
              pantryPunt = true;
            }
          }
        }
      }
    }

    var pantrySrsValue = "7301146011";
    var srsParams = regexSrs.exec(unescape(cwurl));
    areParamAndValuePresent(srsParams, "srs", pantrySrsValue);

    var pantrySearchAliasValue = "pantry";
    var searchAliasParams = regexSearchAlias.exec(unescape(cwurl));
    areParamAndValuePresent(searchAliasParams, "search-alias", pantrySearchAliasValue);

    if (pantryPunt) {
      return true;
    }

    if(sortstring != null &amp;&amp; sortstring.length != 0) {
      var sortParams = cw_extractParams(sortstring);         
      for (var x = 0; x &lt; sortParams.length; x++) { 
        var entry, sortVal;
        entry = sortParams[x].split("=");
        if (entry.length == 2) {
          sortVal = entry[1];
          if(sortVal != "relevancerank" &amp;&amp; sortVal != "") {
            return true;
          }
        }  
      }
    }

    return false;
  }
                  

                
var getSeenAsins = function(keyword) {
  if (typeof atfAdAsinList !== 'undefined') {
     window.pa_asins_tower = atfAdAsinList;
  } else if((typeof(window.pa_bl_tower) != "undefined" &amp;&amp; window.pa_bl_tower != bl) || 
    (typeof(window.pa_kw_tower) != "undefined" &amp;&amp; window.pa_kw_tower != keyword)) {
    window.pa_asins_tower = undefined;
  } else if(typeof(asinList) != "undefined") {
    if(asinList == 0) {
    jQuery('#click_withinLazyLoad_tower').html("&lt;div&gt;&lt;/div&gt;");
    return;
  } else if (typeof(window.pa_asins_tower) != "undefined") { 
    window.pa_asins_tower = window.pa_asins_tower + asinList;                       
  } else {
    window.pa_asins_tower = asinList; 
  }
                    
  var temp = window.pa_asins_tower.split(",");
    if(temp.length &gt; 40 ) {
      var extra = temp.length - 40; 
      for (var i = 0; i &lt; extra; i++) {
        temp.shift();
      }
    } 
    window.pa_asins_tower = temp.join(); 
  }  
}



                var getNodeParameter = function(path, excludeHidden) {
                  var suggestedBnUrl = regexRh.exec(path);
                  var suggestedBnParams = cw_extractParams(suggestedBnUrl);
                  for (var i = 0; i &lt; suggestedBnParams.length; i++) {
                    var sEntry;
                    sEntry = suggestedBnParams[i].split(":");
                    if (sEntry.length === 2 &amp;&amp; sEntry[0] === 'n') {
                      if (!excludeHidden) {
                        return sEntry[1];
                      } else if (sEntry[1].length &gt; 0 &amp;&amp; sEntry[1][0] != '!') {
                        return sEntry[1];
                      }
                    }
                  }
                }

                var bl;
                if (jQuery.pageContext &amp;&amp; jQuery.pageContext.browseLadder) {
                  bl = unescape(jQuery.pageContext.browseLadder);
                }    

                if (shouldPunt(bl)) {
                  jQuery('#click_withinLazyLoad_tower').html("&lt;div&gt;&lt;/div&gt;");
                  return;
                }
             
                var isBrowse = 0;

                var suggestedBN = [];
                if (bl === 'i:aps') {
                  if (jQuery('.categoryRefinementsSection ul li a').length &gt; 0) {
                    var urlattr = jQuery('.categoryRefinementsSection ul li a').attr('href');
                    suggestedBN.push(getNodeParameter(urlattr, false));        
                  } else {
                    var urlattr = jQuery("#refinements &gt; ul &gt; li &gt; a:first").attr("href");  
                    suggestedBN.push(getNodeParameter(urlattr, false));                                       
                  }
                }

                var bnlist = bl.split(',');
                var bn = bnlist[0].replace(":","");          
                var kw = iss.keyword();
                var sa = SearchPageAccess.searchAlias();
                if (!kw) {
                  if (jQuery(".categoryRefinementsSection strong").length &gt; 0) {
                    kw = jQuery(".categoryRefinementsSection strong").html();
                  } else {
                    kw = jQuery("#refinements &gt; ul &gt; li &gt; strong").html();
                  }
                  
                  isBrowse = 1;
                }

                var childBrowseNodes = [];
                if (jQuery(".refinementNodeChildren &gt; li &gt; a").length &gt; 0) {
                  var childHref = jQuery(".refinementNodeChildren &gt; li &gt; a")[0].href;
                  childBrowseNodes.push(getNodeParameter(childHref, true));
                } else {
                  var currentCategory = jQuery("#refinements &gt; ul &gt; li &gt; a &gt; span.boldRefinementLink");

                  if (!currentCategory || currentCategory.length == 0) {
                    currentCategory = jQuery("#refinements &gt; ul &gt; li &gt; strong");
                  }

                  var childHref;
                  if (currentCategory) {
                    childHref = currentCategory.parent().next().find("a&gt;span").first().parent().attr('href');
                  }

                  if (!isBrowse &amp;&amp; childHref) {
                    childBrowseNodes.push(getNodeParameter(childHref, true));
                  }
                }

                getSeenAsins(kw);

                window.pa_bl_tower = bl; 
                window.pa_kw_tower = kw;
                window.pa_sa_tower = sa;

                

                if (!(browseNodeMap.hasOwnProperty(bn) || bl === 'i:aps') || !kw)                        
                {               
                  jQuery('#click_withinLazyLoad_tower').html("&lt;div&gt;&lt;/div&gt;"); 
                  return;
                }

                generateAjaxCall(kw, sa, suggestedBN, window.pa_asins_tower, isBrowse, '0', 
                  'Search', 'portal-batch-slow-btf', 'tower', '0', bl, childBrowseNodes);
              });
          });
        });
      });
    }
  

  
//]]>
                  </script>
                </div>
              </div>
            </div>
