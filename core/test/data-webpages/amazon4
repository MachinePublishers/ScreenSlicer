            &lt;ul class='&lt;#=list.elementClass #&gt;'&gt;
            &lt;# jQuery.each(list.linkList, function (k, link) { #&gt;
              &lt;# if (k === 0) { link.elementClass += ' nav_sv_fo_v_first'; } #&gt;
