    <script type="text/javascript">
//<![CDATA[

var _sf_async_config={};
/** CONFIGURATION START **/
_sf_async_config.uid = 25328;
_sf_async_config.domain = "thefrisky.com";
if ( typeof chartbeat_category != "undefined" ) {
	_sf_async_config.sections = chartbeat_category + ",main"; // Added in "main" to segement from ITS
}
if ( typeof chartbeat_author != "undefined" ) {
	_sf_async_config.authors = chartbeat_author;
}
/** CONFIGURATION END **/
(function(){
	function loadChartbeat() {
		window._sf_endpt=(new Date()).getTime();
		var e = document.createElement("script");
		e.setAttribute("language", "javascript");
		e.setAttribute("type", "text/javascript");
		e.setAttribute("src",
		(("https:" == document.location.protocol) ?
			"https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" :
			"http://static.chartbeat.com/") +
			"js/chartbeat_pub.js");
		document.body.appendChild(e);
	}
	var oldonload = window.onload;
	window.onload = (typeof window.onload != "function") ?
	loadChartbeat : function() { oldonload(); loadChartbeat(); };
})();

//]]>
    </script>
    <div id="fb-root">
    </div>
    <script type="text/javascript">
//<![CDATA[

        jQuery(document).ready(function(){
                  FB.init({appId: '224922937561576', status: true,
                           cookie: true, xfbml: true, oauth: true});
              });
                    
        function insertParam(key, value)
        {
            key = escape(key); value = escape(value);
            var kvp = document.location.search.substr(1).split('&amp;');

            var i=kvp.length; var x; while(i--) 
            {
                x = kvp[i].split('=');

                if (x[0]==key)
                {
                        x[1] = value;
                        kvp[i] = x.join('=');
                        break;
                }
            }

            if(i&lt;0) {kvp[kvp.length] = [key,value].join('=');}

            return kvp.join('&amp;'); 
        }

        function fb_start_login()
        {

            FB.login(function(response) {
				if (response.authResponse) {
                   
                    var date = new Date();
                    date.setTime(date.getTime()+(response.authResponse.expiresIn*1000));
                    var expires = "; expires="+date.toGMTString();
                    document.cookie = "wp_fbs="+response.authResponse.accessToken+expires+"; path=/";
                    
                    var param = insertParam('fb_login', '1');
                    
                    if( jQuery('#modal-login input[name="redirect_to"]') != undefined 
                        &amp;&amp; jQuery('#modal-login input[name="redirect_to"]').attr("value") != undefined    
                        &amp;&amp; jQuery('#modal-login input[name="redirect_to"]').attr("value").length &gt; 0){
                    	param = param + insertParam('redirect_to',  jQuery('#modal-login input[name="redirect_to"]').attr("value"));
                    }
                    	
                    document.location.search = param;
				} 
             }, {scope: ''});
            
        }
          
        
//]]>
    </script>
