  //Get query strings in URL via https://gist.github.com/1771618
  function getURLByParameter(key){
    var result = new RegExp(key + "=([^&amp;]*)", "i").exec(window.location.search);
    return result &amp;&amp; unescape(result[1]) || "";
  }

