    <script type="text/javascript">
//<![CDATA[

  // Setup Ad Targeting Variables
  console.log('setup collected');
  var ad_target_url = "http://oascentral.thedailymeal.com/RealMedia/ads/";
  var ad_target_query = ""; // &amp; Separated
  var ad_target_target = "_blank";
  var ad_target_rn = new String(Math.random());
  var ad_target_rns = ad_target_rn.substring(2, 11);
  var ad_target_listpos = parsePositions({"Top":"","Right":"","Right2":"","Right3":"","Bottom":"","Bottom1":"","Frame1":"","Video":0});

  (function() {
    // Setup Welcome Ad Variables
    var welcome_ad = 0;
    var welcome_ad_frequency = "frequency_once_daily";

    // Set Welcome Ad Frequency Cap
    var wa_hours,wa_minutes;
    if (welcome_ad_frequency == "frequency_three_daily") {
        wa_hours = 8;
        wa_days = 0;
    } else if (welcome_ad_frequency == "frequency_twice_daily") {
        wa_hours = 12;
        wa_days = 0;
    } else {
        wa_hours = 0;
        wa_days = 1;
    }

    // Jump to the Welcome Ad
    if (!getCookie("tdm_welcome") &amp;&amp; welcome_ad == 1) {
      setCookie("tdm_welcome",document.URL,wa_days,wa_hours);
      this.location="/ads/welcome.html";
    }

    // Setup Video Variables
    if (ad_target_listpos.indexOf(",Video")&gt;-1) {
      ad_target_listpos = ad_target_listpos.replace(",Video","");
      video.ova = true;
    } else {
      video.ova = false;
    }
  })();

  // Setup OAS Vars
  var OAS_sitepage = ad_target_sitepage;
  var OAS_query = ad_target_query;

  document.write("&lt;scr" + "ipt type=\"text/javascript\" src=\"" + ad_target_url + "adstream_mjx.ads/" + ad_target_sitepage + "/1" + ad_target_rns + "@" + ad_target_listpos + "?" + ad_target_query + "\"&gt;&lt;\/script&gt;");

  function OAS_AD(pos) {
   //If Add Position is on call OAS_RICH()
      if ((navigator.userAgent.indexOf("Mozilla/3") != -1) || (navigator.userAgent.indexOf("Mozilla/4.0 webTV") != -1)) {
        document.write("&lt;a href=\"" + ad_target_url + "click_nx.ads/" + ad_target_sitepage + "/1" + ad_target_rns + "@" + ad_target_listpos + "!" + pos + "?" + ad_target_query + "\" target=\"" + ad_target_target + "\"&gt;");
        document.write("&lt;img src=\"" + ad_target_url + "adstream_nx.ads/" + ad_target_sitepage + "/1" + ad_target_rns + "@" + ad_target_listpos + "!" + pos + "?" + ad_target_query + "\" border=\"0\" /&gt;");
        document.write("&lt;/a&gt;");
      } else {
        if (typeof OAS_RICH == 'function') OAS_RICH(pos);
      }
  }

  /* parsePositions(jsonObj)
   *
   * Determine which ads run based on frequency cap settings.
   */
  function parsePositions(jsonObj) {
    var list = "";
    var currentPosition = "";
    //console.log("JsonObj: "+jsonObj["Middle2"]);
    if (jsonObj["Middle2"]==1) {
      list = "Middle2";
    } else {
    for (var x in jsonObj) { //for each position
        if (x.indexOf("ros") != -1) {
            currentPosition = x;
        }
        else {
            currentPosition = "content-" + x;
        }
        if(typeof(Storage)!=="undefined") { //if the browser supports local storage
            if (parseInt(jsonObj[x]) &gt; 0) { //and if frequency cap is a non-zero number then increment local storage
                //console.log(currentPosition);
                if (localStorage[currentPosition]) {
                    localStorage[currentPosition] = Number(localStorage[currentPosition]) + 1; //add 1 to the positions view count
                }
                else {
                    localStorage[currentPosition] = 1; //make new variable and set it to 1
                }
                /*ad to the list if not capped*/
                if (parseInt(localStorage[currentPosition]) &lt;= jsonObj[x]) {
                    list = list + x + ",";
                }
            }
            else { //otherwise delete the local storage key/value if it exists and add position to the list
                //delete page-specific value
                if (localStorage.getItem(currentPosition) != null) {
                    localStorage.removeItem(currentPosition);
                }
                //delete run-of-site value if present
                if (localStorage.getItem("ros-" + x) != null) {
                    localStorage.removeItem("ros-" + x);
                }
              list = list + x + ",";
            }
        }
        else { //on older browsers fall back to cookies.
            if (parseInt(jsonObj[x]) &gt; 0) { //and if frequency cap is a non-zero number then increment local storage
                //console.log(currentPosition);
                if (getCookie(currentPosition)) {
                    setCookie(currentPosition, Number(getCookie(currentPosition)) + 1); //add 1 to the positions view count
                }
                else {
                    setCookie(currentPosition,1); //make new variable and set it to 1
                }
                /*ad to the list if not capped*/
                if (parseInt(getCookie(currentPosition)) &lt;= jsonObj[x]) {
                    list = list + x + ",";
                }
            }
            else { //otherwise delete the local storage key/value if it exists and add position to the list
                //delete page-specific value
                if (getCookie(currentPosition)) {
                    deleteCookie(currentPosition);
                }
                //delete run-of-site value if present
                if (getCookie("ros-" + x)) {
                    deleteCookie("ros-" + x);
                }
              list = list + x + ",";
            }
        }
    }//for loop
    list = list.slice(0,-1); //remove trailing comma
    }
    return(list);
  }//parsePositions


  /* setCookie / getCookie / deleteCookie
   *
   *  Helper functions to set/get cookies.
   */
  function setCookie(name,value,days,hours) {
      if (!days &amp;&amp; days != 0) {
          days = 15;
      }
      if (!hours) {
          hours = 0;
      }
      var expDate = new Date();
      expDate.setDate(expDate.getDate() + days);
      expDate.setHours(expDate.getHours() + hours);
      var expires = expDate.toUTCString();
      document.cookie = name + "=" + value + ";expires=" + expires + "; path=/";
  }

  function getCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(";");
    for(var i=0;i &lt; ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==" ") c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
  }

  function deleteCookie(name) {
      setCookie(name,"",-1);
  }

  // execute OAS post-initialization callbacks if any
  if (typeof OAS_onexecute != "undefined") {
    for (var i = 0, j = OAS_onexecute.length; i != j; i++) { 
       try { OAS_onexecute[i](); } catch(err) {}
    }
  }
  OAS_onexecute = { push : function (cb) { cb() } };
  
//]]>
    </script>
