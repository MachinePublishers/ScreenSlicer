              google.load('search', '1', {language : 'en', style : google.loader.themes.STANDARD});
              google.setOnLoadCallback(function() {
                var customSearchControl = new google.search.CustomSearchControl('017445349047153807921:mm_sbf-ghp8');
                customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);
                customSearchControl.draw('cse');
                customSearchControl.execute(getQuery());
              }, true);
              
              function getQuery() {
                var url = '' + window.location;
                var queryStart = url.indexOf('?') + 1;
                if (queryStart &gt; 0) {
                  var parts = url.substr(queryStart).split('&amp;');
                  for (var i = 0; i &lt; parts.length; i++) {
                    if (parts[i].substr(0, 1) == 's') {
                      return unescape(parts[i].split('=')[1].replace(/\+/g, ' '));
                    }
                  }
                }
                return '';
              }
            
