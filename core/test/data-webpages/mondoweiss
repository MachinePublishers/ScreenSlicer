//<![CDATA[

function mondoweiss_hide_tabs( exception ) {
    var contents = jQuery( '#tab_contents &gt; li' );
    if( jQuery( contents ).length &gt; 0 ) {
      jQuery( contents ).each( function( index ) { if( index &gt; exception ) jQuery( this ).hide(); } );
    }
}
jQuery( document ).ready( function() { 
        mondoweiss_hide_tabs( 0 );
        
    var buttons = jQuery( '#tab_buttons li a' );
    if( jQuery( buttons ).length &gt; 0 ) {
      jQuery( buttons ).click( function() {
          mondoweiss_hide_tabs( -1 );
          jQuery( '#'+jQuery( this ).attr( 'rel' ) ).show();
          return false;
        }
      );
    }
    
    if( jQuery('#sharebox').length &gt; 0 ) {    
			var theLoc = jQuery('#sharebox').position().top;
			jQuery(window).scroll(function() {
				if(theLoc &gt;= jQuery(window).scrollTop()) {
					if(jQuery('#sharebox').hasClass('floating')) {
						jQuery('#sharebox').removeClass('floating');
					}
				} else { 	
					if(!jQuery('#sharebox').hasClass('floating')) {
						jQuery('#sharebox').addClass('floating');
					}
				}
			});
			
					}  
    
    jQuery(window).scroll();       
    jQuery(window).resize();
    
  }
);  

//]]>
    </script>
