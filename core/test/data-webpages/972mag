    var tt=false;
      function dispResults(response) {
           tt=response;
           if (tt.queries!=undefined &amp;&amp;
          		 tt.queries.nextPage!=undefined){
     		 jQuery(document).ready(function(){
     			 jQuery('.nav-next').css({'display':'block'});
          		 });        	           		 
           }

           if (tt.queries!=undefined &amp;&amp;
          		 tt.queries.previousPage!=undefined){
     		 jQuery(document).ready(function(){
     			 jQuery('.nav-previous').css({'display':'block'});
          		 });        	           		 
           }
    
           if (tt.searchInformation.totalResults&lt;=0){
                jQuery('#s_titleg').html('Sorry, no results found');
                return;
           }
           else{
          	 jQuery('#s_titleg').html('SEARCH RESULTS:');
          	 jQuery('#s_titleg_num').html(tt.searchInformation.totalResults+' Relevant results found, showing results '+tt.queries.request[0].startIndex+' - '+
                    	 (tt.queries.request[0].startIndex+tt.queries.request[0].count)     	 );
           }
     
      for (var i = 0; i &lt; response.items.length; i++) {
        var item = response.items[i];
        var html='&lt;div class="s_res"&gt;';
        var fGtitle=item.title;
        if (fGtitle.indexOf('| +972 Magazine')&gt;0){
     	   fGtitle=fGtitle.substr(0,fGtitle.indexOf('| +972'));
        }
        html+='&lt;h3&gt;&lt;a href="'+item.link+'"&gt;'+ fGtitle+'&lt;/a&gt;&lt;/h3&gt;';
        html+='&lt;p&gt;'+item.htmlSnippet+'&lt;/p&gt;';
        html+='&lt;a class="sr_link" href="'+item.link+'"&gt;'+item.htmlFormattedUrl+'&lt;/a&gt;';          
        html+='&lt;/div&gt;';
        jQuery('#results').append(html);

      }
    }
    
