    <script type="text/javascript">
//<![CDATA[

function setGroupDescr()
{
if (groupList == null || groupList.length == 0) return;
var table = document.getElementById("groupDetailTable");
//remove all the previous contents
for (var i = table.rows.length - 1; i >= 0; i--) {
table.deleteRow(i);
}
//convert the groupList into a 2D array
var gnList2 = new Array();
var descrList2 = new Array();
var gName2Temp = new Array();
for (var i = 0; i < groupList.length; i++) {
if (i % 2 == 0 && gName2Temp.length > 0) {
gnList2.push(gName2Temp);
gName2Temp = new Array();
}
else {
//do nothing
}
gName2Temp.push(groupList[i]);
}
//take care of the last entry
if (gName2Temp.length > 0) {
gnList2.push(gName2Temp);
}
for (var i = 0; i < gnList2.length; i++) {
var row = table.insertRow(table.rows.length);
var cell0 = row.insertCell(0);
setGroupNameCell(cell0, gnList2[i][0].name, gnList2[i][0].display);
var cell1 = row.insertCell(1);
if (gnList2[i][1] != null) {
setGroupNameCell(cell1,
gnList2[i][1].name,
gnList2[i][1].display);
}
else {
cell1.innerHTML = "&nbsp;";
}
var descrRow = table.insertRow(table.rows.length);
var descrCell0 = descrRow.insertCell(0);
setGroupDescrCell(descrCell0, descrList2[i][0]);
var descrCell1 = descrRow.insertCell(1);
if (descrList2[i][1] != null) {
setGroupDescrCell(descrCell1, descrList2[i][1]);
}
else {
descrCell1.innerHTML = "&nbsp;";
}
}
}
function setGroupNameCell(cell, groupName, displayName) {
var displayStr = "<a href=\"javascript:searchTabFunc('" +
groupName + "');\">" + displayName +
"</a>";
cell.innerHTML = displayStr;
cell.style.textAlign = "left";
cell.style.padding = "5px 2px 2px 5px";
cell.style.fontWeight = "bold";
cell.style.width = "300px";
}
function setGroupDescrCell (cell, descr) {
cell.innerHTML = descr;
cell.style.padding = "2px 2px 2px 10px";
cell.style.textAlign = "left";
cell.style.verticalAlign = "top";
cell.style.width = "300px";
}
var ulStyle = "border-width:1px 0px 0px 0px; border-style:solid; border-color:rgb(180,180,180); padding-top:5px";
function onReady() {
document.forms["searchForm"].elements["q"].disabled = false;
document.forms["searchForm"].elements["q"].focus();
ORACLE.ses.fire('beforeBoot');
var SV = ORACLE.ses.SearchView,
Util = ORACLE.ses.util;
//
// initialize components
//
// SES connection factory
sesConnFactory = new ORACLE.ses.util.SESConnectionFactory({
baseurl: SV.pageVariable('baseurl'),
routingParams: Util.arrayFromJSON(SV.pageVariable('routingParams'))
});
// create an overlay window for Browse Tree
var brsOverlay = new ORACLE.ses.AnchoredOverlay( "browseOverlay" );
brsOverlay.setAnchor( "popupAnchor", "tr", "br" );
brsOverlay.render();
SV.registerBehavior( "ses_toggleBrowseOverlay", function() {
brsOverlay.toggleVisibility();
} );
// search view
sesSV = new ORACLE.ses.SearchView({
searchFormName: 'searchForm'
});
var resultsEncQuery = SV.pageVariable('encPrefQuery');
if( resultsEncQuery == '' )
resultsEncQuery = SV.pageVariable('encQuery');
// results view
sesRV = new ORACLE.ses.ResultsView( sesConnFactory, {
searchView: sesSV,
loadingImg: 'loadingIcon',
resultsListContainer: 'divResults',
query: SV.pageVariable('query'),
encQuery: resultsEncQuery,
displayQuery: SV.pageVariable('displayQuery'),
queryId: SV.pageVariable('qid'),
sourceGroups: Util.arrayFromJSON(SV.pageVariable('encGroups')),
searchWithinInfo: Util.objFromJSON(SV.pageVariable('swnode')),
secureVisibility: SV.pageVariable('secureVisibility'),
simDocHandling: SV.pageVariable('simDocs'),
extraReqParams: SV.pageVariable('extraReqParams'),
enableRemoveResult: ("true" == SV.pageVariable('enableRemoveResult')),
autoLoadTopNResults: ("true" == SV.pageVariable('autoLoadTopNResults','true')),
extendedTimeout: ("true" == SV.pageVariable('extendedTimeout','false')),
startNum: SV.pageVariable('startnum'),
endNum: SV.pageVariable('endnum'),
pageSize: parseInt(SV.pageVariable('hitsPerPage')),
maxNumResults: parseInt(SV.pageVariable('maxNumResults')),
defaultTopN: parseInt(SV.pageVariable('defaulttopn')),
maxEmailThreadDocs: parseInt(SV.pageVariable('maxEmailThreadDocs')),
maxTopN: parseInt(SV.pageVariable('maxtopn'))
});
sesRV.set('paginatorContainers', 'divPageLinks');
sesRV.set('groupSortContextMsgEl', 'groupSortContextMsg');
sesRV.set('hitReportEl', 'hitReport');
sesRV.set('groupingEl', 'divGroup');
sesRV.set('sortingEl', 'divSort');
sesRV.set('taggingEnabled', false);
if ("true" == SV.pageVariable('searcherror'))
sesRV.set('isSearchErr', true);
else if ("true" == SV.pageVariable('noresults'))
sesRV.set('isNoResults', true);
// cluster view
//sesCV = new ORACLE.ses.ClusterView( 'divClusterTree', {
// displayEmptyTrees: ("true" == SV.pageVariable('showEmptyClusters','true')),
// moreLimit: parseInt(SV.pageVariable('clusterMoreLimit'))
// });
//sesCV.set('removeFilterId', 'navRemoveFilter');
//repace the cluster view with the customized
//contents that the user would implement by himself
var divClusterTree = document.getElementById("divClusterTree");
if (divClusterTree) {
divClusterTree.innerHTML = "Customize Me";
customizeSideBar();
}
// inline attribute filters
sesIAF = new ORACLE.ses.InlineAdvancedView( "divInlineAdvSearch", "attrnames" );
/* === helper methods === */
var arrIndexOf = function(arr, item) {
for (var i=0, len=arr.length; i<len; ++i) {
if (arr[i] === item) return i;
}
return -1;
};
var filterAttrs = function(attrArr, exclArr) {
var arr = [];
for (var i=0, len=attrArr.length; i<len; ++i) {
if (arrIndexOf(exclArr, attrArr[i].NAME.toLowerCase()) == -1)
arr.push(attrArr[i])
}
return arr;
};
/* === list of (lowercase) attribute names to suppress === */
var suppressAttrs = [
"additionalattribute",
"c_rptno",
"collectionid",
"ddocaccount",
"data_source",
"dayofweek",
"ddocauthor",
"ddocname",
"ddoctitle",
"ddoctype",
"did",
"dindate",
"doriginalname",
"doutdate",
/* "dreleasedate", */
"drevlabel",
"dsecuritygroup",
"groupname",
"handle",
"headline1",
"headline2",
"headline3",
"key",
"person_image",
"perspectives",
"reference text",
"scehostname",
"searchimage",
"tag",
"username",
"urldepth",
"wwsbr_category_",
"wwsbr_charset_",
"wwsbr_itemtype_",
"wwsbr_mime_type_",
"wwsbr_name_",
"wwsbr_publishdate_",
"wwsbr_subtype_",
"wwsbr_updatedate_",
"wwsbr_updator_",
"wwsbr_version_number_"
];
// do the filtering
ORACLE.ses.InlineAdvancedView._attrNamesArr =
filterAttrs(ORACLE.ses.InlineAdvancedView._attrNamesArr, suppressAttrs);
// suggested content
sesSCV = new ORACLE.ses.SuggestedContentView( 'sugg-info-container', sesConnFactory, {
encQuery: SV.pageVariable('encQuery'),
secureVisibility: SV.pageVariable('secureVisibility')
});
YAHOO.lang.later( 0, sesSCV, 'getSuggestedContent');
// browse view for browse tree
sesBV = new ORACLE.ses.BrowseView( "divBrowseTree", sesConnFactory, {
statusEl: 'divBrowseStatus',
secureVisibility: SV.pageVariable('secureVisibility'),
prefetchBrowseGroups: ("true" == SV.pageVariable('prefetchBrowseGroups'))
});
ORACLE.ses.fire('beforeWirePhase');
//
// wire components together
//
sesBV.get('model').subscribe('browseUpdate', sesSV.onBrowseUpdate, sesSV, true);
sesBV.get('model').subscribe('browseUpdate', sesIAF.onBrowseUpdate,sesIAF,true);
sesBV.get('model').subscribe('browseOff', sesSV.onBrowseOff, sesSV, true);
sesBV.get('model').subscribe('browseOff', sesIAF.onBrowseOff, sesIAF,true);
sesBV.subscribe('allDocsClick', sesSV.onAllDocsClick, sesSV, true);
sesSV.subscribe('clearBrowse', sesBV.onClearBrowseClick, sesBV, true);
sesSV.subscribe('clearBrowse', function() {
sesIAF.onBrowseUpdate(sesBV.get('model'));
});
brsOverlay.showEvent.subscribe(sesBV.onShow, sesBV, true);
//sesRV.get('model').subscribe('newResults', sesCV.renderCluster, sesCV, true);
ORACLE.ses.fire('beforeRenderPhase');
// render!
sesRV.render();
sesBV.render();
setDebugParam();
setGroupParam();
ORACLE.ses.fire('bootDone');
};
/**
* function to replace the original size bar tree structure
* for demo only.
*/
function customizeSideBar()
{
//remove the status bar
clearSideBar();
//create the group area
setGroupArea();
//update the current custom filter value array
//setCustomFilterValues();
//create the correponding 2nd level condition
//setCategories();
//set the side bar to be visible all the time
var sidebarDiv = $("sidebar");
if (sidebarDiv.style.display == "none") {
sesSV.toggleSidebar();
}
};
/**
* function to remove the contents in the side bar
*/
function clearSideBar()
{
var statusDiv = document.getElementById("divSidebarStatus");
if (statusDiv) {
statusDiv.style.display = "none";
}
var divClusterTree = document.getElementById("divClusterTree");
if (divClusterTree) {
divClusterTree.style.display = "block";
}
var filterTitle = document.getElementById("filterTitle");
if (filterTitle) filterTitle.innerHTML = "";
//var hitReportSpan = document.getElementById("hitReport");
//if (hitReportSpan) hitReportSpan.innerHTML = "&nbsp;";
};
/**
* function to populate the group area in the side bar
*/
function setGroupArea()
{
var groupNames = [
"All"
,"Blogs"
,"Corporate Site"
,"C-Central"
,"Discussion Forums"
,"Documentation"
,"EventsRSS"
,"Java.com"
,"JavaOne"
,"MySQL"
,"Oracle OpenWorld"
,"PartnerNetwork"
,"Technology Network"
,"Training"
,"bugs.sun.com"
,"testme"
];
//get curent group id or name
var grpstr = ORACLE.ses.SearchView.pageVariable("groups");
var selectedGroups = [];
if (grpstr != "") {
selectedGroups = YAHOO.lang.JSON.parse(decodeURIComponent(grpstr));
}
var html = ""
html += "<ul>";
for(var i=0; i<groupNames.length; i++)
{
if(groupNames[i] == "testme" || groupNames[i] == "EventsRSS" || groupNames[i] == "Webtech Marketing")
continue;
var sel = false;
for(var j=0; j<selectedGroups.length; j++)
{
if(selectedGroups[j] == groupNames[i])
{ sel = true;
break;
}
}
if(sel)
{
html += "<li class=\"leftNavSrcGrp\" style=\"font-weight:bold; color:#FF0000;\">" + groupNames[i] + "</li>";
}
else
{
html += "<li class=\"leftNavSrcGrp\"><a href=\"javascript:searchTabFunc('" + groupNames[i] + "');\">" + groupNames[i] + "</a></li>";
}
}
html += "</ul>";
var divClusterTree = document.getElementById("divClusterTree");
if (divClusterTree) divClusterTree.innerHTML = html;
};
/**
* function to populate the category or attribute area
* in the side bar
*/
function setCategories()
{
//create a container to hold the hard coded categories
var catDivObj = document.createElement("div");
catDivObj.id = "sideBarCustCatDiv";
var sidebarDiv = document.getElementById("divCluster");
sidebarDiv.appendChild(catDivObj);
//set the contents based on the selected group
//get curent group id or name
var grpstr = ORACLE.ses.SearchView.pageVariable("groups");
var selectedGroups = [];
if (grpstr != "") {
selectedGroups = YAHOO.lang.JSON.parse(decodeURIComponent(grpstr));
}
var selGroup = "";
if (selectedGroups.length > 0) {
selGroup = selectedGroups[0];
}
var catHTML = "";
if (selGroup == "Bugs") {
catHTML = getBugHTML();
}
else if (selGroup == "MyOracle") {
catHTML = getSiteHTML();
}
else if (selGroup == "Dev") {
catHTML = getDevHTML();
}
else {
catHTML = getTimeHTML();
}
catDivObj.innerHTML = catHTML;
};
/**
*
*/
function setCustomFilterValues()
{
var customFilterValStr = getQueryParamValue("customFilterValues");
if(customFilterValStr == null || customFilterValStr == "")
{ return;
}
var formObj = document.searchForm;
var cfvInput = formObj.customFilterValues;
if(cfvInput == null)
{ addHiddenInput("customFilterValues", customFilterValStr, formObj);
}
else
{ cfvInput.value = customFilterValStr;
}
};
/**
*
*/
function getTimeHTML()
{
var types = ["Any Time", "Past Week", "Past Month", "Past Year", "Past Three Years"];
var html = getListBlock(types, "searchByDate", 9, "date", "date");
return html;
};
/**
*
*/
function getSiteHTML()
{
var prodTypes = ["Any Site", "Alliances & Channels", "Cloud Services", "Customer Services", "Finance & Administration",
"Global Business Units", "Human Resources", "Industries Business Unit", "Information Technology", "Japan", "Legal",
"Marketing", "Oracle University", "Oracle R&D Centers", "Product Development", "Real Estate & Facilities",
"Sales and Consulting", "Security"];
var html = getListBlock(prodTypes, "searchByMyOracleSite", 1, "MyOracle", "Website");
html += getTimeHTML();
return html;
};
/**
*
*/
function getBugHTML()
{
var prodTypes = ["Any Product", "RDBMS", "PLSQL", "Oracle Text", "SES"];
var statusTypes = ["Any Status", "Open", "Closed"];
var html = "<br /><b>Products: </b><br />" + getListBlock(prodTypes, "searchByBugProd", 1, "bug", "product");
html += "<br /><b>Status: </b><br />" + getListBlock(statusTypes, "searchByBugStatus", 2, "bug", "status");
html += getTimeHTML();
return html;
};
/**
*
*/
function getDevHTML()
{
var devTypes = ["All Products", "RDBMS", "SES"];
var html = "<br /><b>FS for Product</b><br />";
html += getListBlock(devTypes, "searchByDevFS", 1, "dev", "product");
html += getTimeHTML();
return html;
};
/**
*
*/
function getListBlock(typeList, funcName, attributeIndex, sourceGroupName, filterName)
{
if(typeList == null)
return "";
var filterArrayStr = getQueryParamValue("customFilterValues");
filterArrayStr = decodeURIComponent(filterArrayStr).replace(/\+/g, " ");
var filterArray = filterArrayStr.split(":");
var selType = "";
for(var i = 0; i < filterArray.length; i++)
{
var fv = filterArray[i].split(",");
if(fv[0] == sourceGroupName && fv[1] == filterName)
{ selType = fv[2];
}
}
if(selType == "")
selType = typeList[0];
//maybe it is from different category..
var found = false;
for(var i = 0; i < typeList.length; i++)
{
if(typeList[i] == selType)
{ found = true;
break;
}
}
if(!found)
selType = typeList[0];
var html = "<ul style=\"" + ulStyle + "\">";
for(var i = 0; i< typeList.length; i++)
{
if(selType == typeList[i])
{
html += "<li style=\"font-weight:bold\">" + typeList[i] + "</li>";
}
else
{
html += "<li><a href=\"javascript:" + funcName + "(" + attributeIndex + ", '" +
typeList[i] + "');\">" + typeList[i] + "</a></li>";
}
}
html += "</ul>";
return html;
};
/**
*
*/
function searchByDate(attrIndex, type)
{
var startDate = new Date();
var startOp = "greaterthanequals";
var attrName = "LastModifiedDate";
var formObj = document.searchForm;
if (type == 'Past Week') {
startDate.setTime(new Date().getTime() - 7*24*60*60*1000);
}
else if (type == 'Past Month') {
startDate.setTime(new Date().getTime() - 30*24*60*60*1000);
}
else if (type == 'Past Year') {
startDate.setTime(new Date().getTime() - 365*24*60*60*1000);
}
else if (type == 'Past Three Years') {
startDate.setTime(new Date().getTime() - 3*365*24*60*60*1000);
}
else // "Any Time" catch-all
{
removeFilter(attrName);
updateCustomFilterValues("date", "date", type, formObj);
formObj.submit();
return;
}
var pStartValue = dateToString(startDate);
setupSearchForm(attrIndex, attrName, startOp, pStartValue);
updateCustomFilterValues("date", "date", type, formObj);
formObj.submit();
};
/**
* not been used
*/
function searchByAuthor(attrIndex, type)
{
var formObj = document.searchForm;
var attrName = "Author";
if (type == "Any Author") {
removeFilter(attrName);
}
else {
setupSearchForm(attrIndex, attrName, "contains", type);
}
addHiddenInput("customFilterValue", type, formObj);
formObj.submit();
};
function searchByMyOracleSite(attrIndex, type)
{
var formObj = document.searchForm;
var attrName = "FolderPath";
var typeId = null;
if(type == "Any Site")
{ removeFilter(attrName);
}
else if(type == "Alliances & Channels")
{ typeId = "/site/anc/";
}
else if(type == "Cloud Services")
{ typeId = "/site/cloud/";
}
if (typeId != null) {
setupSearchForm(attrIndex, attrName, "contains", typeId);
}
updateCustomFilterValues("MyOracle", "Website", type, formObj);
formObj.submit();
};
function searchByBugProd(attrIndex, type)
{
var formObj = document.searchForm;
var attrName = "product";
var typeId = -1;
if (type == "Any Product") {
removeFilter(attrName);
}
else if (type == "RDBMS") {
typeId = "5";
}
else if (type == "PLSQL") {
typeId = "11";
}
else if (type == "Oracle Text") {
typeId = "211";
}
else if (type == "SES") {
typeId = "1737";
}
if (typeId > 0) {
setupSearchForm(attrIndex, attrName, "contains", typeId);
}
updateCustomFilterValues("bug", "product", type, formObj);
formObj.submit();
};
/**
*
*/
function searchByBugStatus(attrIndex, type)
{
var formObj = document.searchForm;
var attrName = "bug status num";
if (type == "Any Status") {
removeFilter(attrName);
}
else if (type == "Open") {
setupSearchForm(attrIndex, attrName, "equals", "11");
}
else if (type == "Closed") {
setupSearchForm(attrIndex, attrName, "greaterthanequals", "70");
}
updateCustomFilterValues("bug", "status", type, formObj);
formObj.submit();
};
/**
* function to update the form's hidden variables and then submit to the server
*/
function setupSearchForm (attrId, attrName, operator, attrValue)
{
var formObj = document.searchForm;
removeFilter(attrName);
//add new filters according to the passed in parameter
//addHiddenInput("search_p_at", attrId, formObj);
addHiddenInput("adn", attrName, formObj);
addHiddenInput("search_p_atname", attrName, formObj);
addHiddenInput("search_p_op", operator, formObj);
addHiddenInput("search_p_val", attrValue, formObj);
//add an empty entry
addHiddenInput("adn", "", formObj);
addHiddenInput("search_p_atname", "", formObj);
addHiddenInput("search_p_op", "equals", formObj);
addHiddenInput("search_p_val", "", formObj);
};
/**
*
*/
function removeFilter(attrName) {
var formObj = document.searchForm;
//remove all the entries related the passed in attrId
var seqIndexList = new Array();
var attrNameInput = formObj['search_p_atname'];
if (attrNameInput) {
if (attrNameInput.length) {
for (var i = 0; i < attrNameInput.length; i++) {
if (attrNameInput[i].value == attrName || attrNameInput[i].value == "") {
var id = attrNameInput[i].id.split("_")[1];
seqIndexList.push(id);
}
}
}
else { //only one attrIDInput is found in the form
if (attrNameInput.value == attrName || attrNameInput.value == "") {
var id = attrNameInput.id.split("_")[1];
seqIndexList.push(id);
}
}
}
//remove all the potential conflict filters and empty inputs
for (var i = 0; i < seqIndexList.length; i++) {
//name=search_p_at, id=attrIdInput_x
var tempID = "attrIdInput_" + seqIndexList[i];
removeInput(tempID);
//attribute name=search_p_atname, id=attrNameInput_x
tempID = "attrNameInput_" + seqIndexList[i];
removeInput(tempID);
//attribute display name name=adn, id=attrDisplayNameInput_x
tempID = "attrDisplayNameInput_" + seqIndexList[i];
removeInput(tempID);
//operator name=search_p_op, id=search_p_op:x
tempID = "search_p_op:" + seqIndexList[i];
removeInput(tempID);
//filter value name=search_p_value id=attrValueInput_x
tempID = "attrValueInput_" + seqIndexList[i];
removeInput(tempID);
}
}
/**
*
*/
function removeInput(inputID) {
var tempInputObj = document.getElementById(inputID);
if (tempInputObj == null) {
return;
}
tempInputObj.parentNode.removeChild(tempInputObj);
tempInputObj = null;
}
/**
*
*/
function updateCustomFilterValues(sourceGroupName, filterName, filterValue, formObj)
{
var currValue = "";
var hiddenInput = formObj.customFilterValues;
if (hiddenInput == null)
{
var valArray = [sourceGroupName, filterName, filterValue];
currValue = valArray.join(",");
addHiddenInput("customFilterValues", currValue, formObj);
}
else
{
var currValue = decodeURIComponent(hiddenInput.value).replace(/\+/g, ' ');
var filterArray = currValue.split(":");
var foundPrevious = false;
for (var i = 0;i < filterArray.length; i++)
{
var filterValArray = filterArray[i].split(",");
if (filterValArray[0] == sourceGroupName &&
filterValArray[1] == filterName) {
filterValArray[2] = filterValue;
filterArray[i] = filterValArray.join(",");
foundPrevious = true;
break;
}
}
if (!foundPrevious)
{
var filterValArray = [sourceGroupName, filterName, filterValue];
var filterValStr = filterValArray.join(",");
filterArray.push(filterValStr);
}
currValue = filterArray.join(":");
hiddenInput.value = currValue;
}
};
/**
*
*/
function addHiddenInput(name, value, formObj) {
var hiddenInput = document.createElement("input");
hiddenInput.type = "hidden";
hiddenInput.name = name;
hiddenInput.value = value;
formObj.appendChild(hiddenInput);
};
/**
*
*/
function getQueryParamValue(paramKey) {
var searchStr = window.location.search.substring(1);
var pairs = searchStr.split("&");
for (var i = 0; i < pairs.length; i++) {
var p = pairs[i].split("=");
if (p.length < 2) continue;
if (p[0] == paramKey) {
p[1] = decodeURIComponent(p[1]).replace(/\+/g, ' ');
return p[1];
}
}
return "";
};
/**
* convert the date object to a string in the format
* MM/DD/YYYY
*
* @param dateValue a Javascript Date object
*/
function dateToString(dateValue) {
var dd = dateValue.getDate();
var mm = dateValue.getMonth() + 1;
var yyyy = dateValue.getFullYear();
return mm + "/" + dd + "/" + yyyy;
};
/**
* function that replace the original searchTabFunc()
*/
function searchTabFunc(sgName)
{
var formObj = document.searchForm;
if (formObj.group == null) {
addHiddenInput("group", sgName, formObj);
}
else {
formObj.group.value = sgName;
}
document.searchForm.submit();
};
function setGroupParam() {
var groupValue = getQueryParamValue("group");
var formObj = document.searchForm;
if (formObj.group == null) {
addHiddenInput("group", groupValue, formObj);
}
else {
formObj.group.value = groupValue;
}
}
/**
* support function for diagnostics to have sticky debug argument
*/
function setDebugParam() {
var debugValue = getQueryParamValue("debug");
if (debugValue == "") return;
var formObj = document.searchForm;
if (formObj.debug == null) {
addHiddenInput("debug", debugValue, formObj);
}
else {
formObj.debug.value = debugValue;
}
};
//check if the groupList is loaded by query.ftl
if ((typeof groupList) != 'undefined') {
setGroupDescr();
}
