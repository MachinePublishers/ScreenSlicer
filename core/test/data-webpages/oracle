//<![CDATA[

function initSearchFormOnload() {
document.forms["searchForm"].elements["q"].disabled = false;
document.forms["searchForm"].elements["q"].focus();
}
YAHOO.util.Event.onDOMReady(initSearchFormOnload);

//]]>
