        <!-- Pagination Links -->        <script type="text/javascript">
//<![CDATA[

function searchPageLink(startnum, endnum)
{
document.forms["searchForm"].elements["search_startnum"].value = startnum;
document.forms["searchForm"].elements["search_endnum"].value = endnum;
document.forms["searchForm"].submit();
//add the current facet path to the submit form
var facetPathsStr = ORACLE.ses.SearchView.pageVariable('facetPaths');
facetPathsStr = decodeURIComponent(facetPathsStr).replace(/\+/g, ' ');
facetPaths = ORACLE.ses.util.arrayFromJSON(facetPathsStr);
if (facetPaths != null) {
for (var i = 0; i < facetPaths.length; i++) {
var inputObj = ORACLE.ses.FacetView.getFacetPathInput(formObj, facetPaths[i]);
if (inputObj == null) {
inputObj = document.createElement("INPUT");
inputObj.type = "hidden";
inputObj.name = "fp";
formObj.appendChild(inputObj);
}
inputObj.value = facetPaths[i];
}
}
formObj.submit();
}
function paginateLocal(key, startnum, endnum)
{
// if paginate() returns true, we're done.. client-side operation.
if( ORACLE.ses.ResultsView.paginate( key, startnum, endnum ) )
return;
// else, submit the form to get page of results from server
searchPageLink(startnum, endnum);
}

//]]>
        </script>
