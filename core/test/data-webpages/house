//<![CDATA[

        window['curwt'] = function(t, ct) {
          try {
            if (t === window) {
              t = window.event.srcElement;
              while (t) {
                if (t.href) {
                  break;
                }
                t = t.parentNode;
              }
            }
            t.href = ct;
            t.onmousedown = '';
          } catch (e) {
          }
          return true;
        }
      
