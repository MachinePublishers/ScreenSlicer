//<![CDATA[

    //Set cookie for backbutton usage
    $(function () {
        var exdate = new Date();
        exdate.setDate(exdate.getDate() + 1);
        var url = encodeURI("/content/www/us/en/search.html?keyword="); //escape() is deprecated!
        var cookieValue = "search20###" + url + ((exdate == null) ? "" : "; expires=" + exdate.toUTCString()) + "; path=/";
        document.cookie = "backbutton=" + cookieValue;
    });

//]]>
      </script>
