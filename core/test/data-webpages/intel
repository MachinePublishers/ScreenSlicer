        $(".sign-in-link .link a").bind("click", function (e) {
            e.preventDefault();
            if (!$(this).hasClass("selected")) {
                $("#sign-in-form").load(signinURL, function () {
                    $(".signin-nav-box").slideDown(function () {
                        $(".sign-in-link .link a").addClass("selected");

                    });
                });
            } else {
                closeSigninBox();
            }
        });
