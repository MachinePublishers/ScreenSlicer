        <script>
//<![CDATA[

(function () {
    function getCookie(c_name) {
        var c_value = document.cookie;
        var c_start = c_value.indexOf(c_name + "=");
        if (c_start == -1) {
            c_start = c_value.indexOf(c_name + "=");
        }
        if (c_start == -1) {
            c_value = null;
        } else {
            c_start = c_value.indexOf("=", c_start) + 1;
            var c_end = c_value.indexOf(";", c_start);
            if (c_end == -1) {
                c_end = c_value.length;
            }
            c_value = unescape(c_value.substring(c_start, c_end));
        }
        return c_value;
    }

    var wpatc = getCookie("WPATC");
    if (wpatc != null && wpatc != "") {
        var n = wpatc.split(":");
        for (i = 0; i < n.length; i++) {
            // get attribute pairs
            var attribute = n[i].toUpperCase();
            var split_value = attribute.split("=");

            switch (split_value[0]) {
                case "A":
                    // set attribute of A
                    var value = split_value[1];
                    Krux('set', {
                        'user_attr_A': value
                    });
                    break;
                case "B":
                    // set attribute of B AND KEEP reading
                    var value = split_value[1];
                    if (value < 25) {
                        Krux('set', {
                            'user_attr_B_Job': value
                        });
                    } else if (value >= 25 && value <= 59) {
                        Krux('set', {
                            'user_attr_B_Responsibility': value
                        });
                    } else if (value >= 60 && value <= 99) {
                        Krux('set', {
                            'user_attr_B_Industry': value
                        });
                    } else if (value >= 100 && value <= 110) {
                        Krux('set', {
                            'user_attr_B_Size': value
                        });
                    }
                   case "D":
                    value = split_value[1];
                    if (value >= 1 && value <= 5) {
                    Krux('set', {
                        'user_attr_D': value
                    });
                    break;
                    }
            }
        }
    }
})();

//]]>
        </script>
