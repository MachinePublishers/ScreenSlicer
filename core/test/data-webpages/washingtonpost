    <script type="text/javascript">
//<![CDATA[

			// Date parsing essentials 
			WPS.Util = {
			 	fmtInt: function(nStr) {
					nStr += '';
					x = nStr.split('.');
					x1 = x[0];
					x2 = x.length > 1 ? '.' + x[1] : '';
					var rgx = /(\d+)(\d{3})/;
					while (rgx.test(x1)) {
						x1 = x1.replace(rgx, '$1' + ',' + '$2');
					}
					return x1 + x2;
				}, 
				reverseDateLookup : function (value){
					for (key in this.data.dateRangeQueries){
						if (this.data.dateRangeQueries[key] === value) { return key; };
					}
					return false;
				},
				data: {
					dateRangeQueries:  {
						"Past 24 Hours" : "displaydatetime:[NOW/DAY-1DAY TO NOW/DAY+1DAY]",
						"Past 7 Days": "displaydatetime:[NOW/DAY-7DAYS TO NOW/DAY+1DAY]",
						"Past 60 Days" : "displaydatetime:[NOW/DAY-60DAYS TO NOW/DAY+1DAY]",
						"Past 12 Months" : "displaydatetime:[NOW/DAY-1YEAR TO NOW/DAY+1DAY]",
						"All Since 2005": "displaydatetime:[2005-01-01T00:00:00Z TO NOW/DAY+1DAY]"
					},
					months : ['JAN', 'FEB', 'MAR', 'APR', 'MAY', 'JUN', 'JUL', 'AUG', 'SEP', 'OCT', 'NOV', 'DEC']
				}
			};
		    // Temp accordion for Post Most
		    $(function(){
			    var handles = $('div.right-rail').eq(0).find('ul.accordion li.title span a');
			    handles.click(function(e){
			    	e.preventDefault();
			    	var target 	 = $(this).parent().parent(),
			    		expanded = target.hasClass('expand');
			    	if (!expanded){
			    		target.removeClass('closed').addClass('expand').removeClass('bkgd-grey-gradient').find('ol').show('slow');
			    	}	
			    	handles.not($(this)).parent().parent().removeClass('expand').addClass('closed').addClass('bkgd-grey-gradient').find('ol').hide('slow');
			    });
		    });
		
//]]>
    </script>
