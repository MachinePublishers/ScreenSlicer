          <script type="text/javascript">
//<![CDATA[


function addCommas(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length &gt; 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}

currentSearchTerm = '';
function ajaxSearch(q, start){
	currentSearchTerm = q;
	itemsPerPage = 8;
	url = 'http://ajax.googleapis.com/ajax/services/search/web?v=1.0&amp;start=' + start + '&amp;q=' + urlencode(q) + '+site:theatlantic.com&amp;rsz=large&amp;callback=?';
	
	$.getJSON(url,function(data){
		output = '';
	
		if (data.responseData.results.length){
			//There are search results
			$.each(data.responseData.results,function(){
				output += '&lt;div class="post"&gt;&lt;h3 class="headline"&gt;&lt;a href="' + this['url'] + '"&gt;' + this['title'] + '&lt;\/a&gt;&lt;\/h3&gt;&lt;p class="dek"&gt;' + this['content'] + '&lt;\/p&gt;&lt;\/div&gt;';
			});
			
			showResults(output);
		
			var responseCount = data.responseData.cursor.estimatedResultCount.toString()
			$("#searchResultsInfoTop").html("Results " + (start + 1) + "-" + (start + itemsPerPage) + "  of about " + addCommas(responseCount));
			pagination = '';
			//Paginate if there are 2 or more pages
			if (data.responseData.cursor.pages.length &gt; 1){
			
				pagination = '&lt;div class="pagination"&gt;&lt;div class="paginationInner"&gt;';
				$.each(data.responseData.cursor.pages,function(){
					
					if (start == this.start){
						pagination += '&lt;span class="currentPage"&gt;' + this.label + '&lt;\/span&gt;';
					}else {

						pagination += '&lt;a href="#" onclick="paginationLink(' + this.start + ');"&gt;' + this.label + '&lt;\/a&gt;';
						
					}
					
					if (this.label == '8'){
							pagination += '&lt;a href="http://www.google.com/search?q=' + q + '+site:theatlantic.com&amp;start=' + (itemsPerPage * 8) + '"&gt;More results &amp;raquo;&lt;\/a&gt;';
					}
					
				});
				pagination += '&lt;\/div&lt;\/div&gt;';
			}
			
			
			$("#searchResultsInfoBottom").html(pagination);
			//updating omniture, post html snippet write so as not to slow things down
			if(s){s.prop2=responseCount; s.t();}
			
		}else {
			//No results
			showResults('&lt;p&gt;Your search - &lt;i&gt;' + q + '&lt;\/i&gt; - did not match any documents. &lt;\/p&gt;');
		}
		
		
	}, "json");
	
	return false;
}

function showResults(output){
	$("#searchResults").html(output);
	$("#searchResultsInfoTop").html(' ');
	$("#searchResultsInfoBottom").html(' ');
}


function paginationLink(start){
	ajaxSearch(currentSearchTerm, start);
	return false;
}

function urlencode(str){
	str = (str+'').toString();
	return encodeURIComponent(str).replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');
}


$(document).ready(function(){
	$("#searchForm").submit(function() {
		window.location.href = "/search/?sort=time&amp;source=search&amp;q="+escape($("#q").val());
		return false;
});
	
	
	ajaxSearch('ibm',0);

});

//]]>
          </script>
