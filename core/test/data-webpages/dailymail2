            <script>
//<![CDATA[

    // Inline script for now
    function searchGotoOffset(offset) {
      var searchForm = $('#searchCommand');    
      $('#searchhiddenoffset').val(offset);
      searchForm.submit();    
      return false;
    }
    function searchSetPageSize(size) {
      var searchForm = $('#searchCommand');
      $('#searchhiddenpagesize').val(size);
      searchForm.submit();
      return false;
    }
    function searchSubmit() {
      var searchForm = $('#searchCommand'),
          searchSite = $('#bing-site');

      if (searchSite.prop('checked')) {
        $('#searchhiddenoffset').val(0);
        searchForm.submit();  
      } else {
        var searchField = $('#searchPhrase')
        window.open('http://www.bing.com/search?q=' + searchField.val() + '&amp;cp=1252&amp;FORM=MOL635', '_blank');
      }
      
      
    }
    function searchByAuthor(author) {
      var searchForm = $('#searchCommand');
      searchForm.append($('&lt;input id="searchhiddenauthor" type="hidden" name="authornamef" value=""&gt;'));
      $('#searchhiddenauthor').val(author);
      searchForm.submit();     
    }
  
//]]>
            </script>
