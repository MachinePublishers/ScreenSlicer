	// BBM Write Ads
	function bbmWriteAd(ad,iframe)
	{
	  	var protocol = (("https:" == document.location.protocol) ? "https://" : "http://");
	  	if (typeof iframe !== 'undefined') {
	    	// Need to output iFrame code
	    	//build urls
	    	var iurl;
	  		var url;

	  		if(iframe.domain == 'adserver1')
  			{
  				iurl = protocol + 'adserver1.backbeatmedia.com/servlet/ajrotator/253/845/viewHTML';
  			} else {
  				iurl = protocol + 'bullseye.backbeatmedia.com/bullseye/adserver/253/291/viewHTML';
  			}

	  		if(ad.domain == 'adserver1')
  			{
  				url = protocol + 'adserver1.backbeatmedia.com/servlet/ajrotator/253/845/viewJScript';
  			} else {
  				url = protocol + 'bullseye.backbeatmedia.com/bullseye/adserver/253/291/viewJScript';
  			}

  			var toWrite = '&lt;ifr' + 'ame src="' + iurl + '?pool=' + ad.pool + '&amp;type=' + ad.type + '&amp;pos=' + ad.pos + '&amp;zone=' + ad.zone + '&amp;redirect=' + ad.redirect + '&amp;dontcount=' + ad.dontcount + '" width="' + iframe.width + '" height="' + iframe.height + '" align="' + iframe.align + '" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"&gt;' + '&lt;sc' + 'ript language="JavaScript" type="text/javascript" src="' + url + '?pool=' + ad.pool + '&amp;type=' + ad.type + '&amp;pos=' + ad.pos + '&amp;zone=' + ad.zone + '&amp;redirect=' + ad.redirect + '"&gt;&lt;\/sc' + 'ript&gt;' + '&lt;\/ifr' + 'ame&gt;';
  			/*
	    	document.write(
	      	'&lt;ifr' + 'ame src="' + iurl + '?pool=' + iframe.pool + '&amp;type=' + iframe.type + '&amp;pos=' + iframe.pos + '&amp;zone=' + iframe.zone + '&amp;redirect=' + iframe.redirect + '" width="' + iframe.width + '" height="' + iframe.height + '" align="' + iframe.align + '" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"&gt;' + '&lt;sc' + 'ript language="JavaScript" type="text/javascript" src="' + url + '?pool=' + ad.pool + '&amp;type=' + ad.type + '&amp;pos=' + ad.pos + '&amp;zone=' + ad.zone + '&amp;redirect=' + ad.redirect + '"&gt;&lt;\/sc' + 'ript&gt;' + '&lt;\/ifr' + 'ame&gt;'
	    	);
			*/
			jQuery(iframe.id).html(toWrite);

	  	} else {
	  		if (typeof ad !== 'undefined') {
	  			//build url
	  			var url;
	  			if(ad.domain == 'adserver1')
	  			{
	  				url = protocol + 'adserver1.backbeatmedia.com/servlet/ajrotator/253/845/viewJScript';
	  			} else {
	  				url = protocol + 'bullseye.backbeatmedia.com/bullseye/adserver/253/291/viewJScript';
	  			}

	    		document.write('&lt;sc' + 'ript language="JavaScript" type="text/javascript" src="' + url + '?pool=' + ad.pool + '&amp;type=' + ad.type + '&amp;zone=' + ad.zone + '&amp;redirect=' + ad.redirect + '&amp;dontcount=' + ad.dontcount + '"&gt;&lt;\/sc' + 'ript&gt;');
	    	}
		}
	}
	
