		ads_cluster["ad_2"] = googletag.defineSlot("/4128/CBS.HARTFORD/" + cbs_ads_zone + "", [[300,250]], "ad_2").addService(googletag.pubads()).setTargeting("tag", cbs_ads_tags).setTargeting("loc", "top").setTargeting("pos", 2);
		ads_cluster["ad_4"] = googletag.defineSlot("/4128/CBS.HARTFORD/" + cbs_ads_zone + "", [[728,90]], "ad_4").addService(googletag.pubads()).setTargeting("tag", cbs_ads_tags).setTargeting("loc", "bottom").setTargeting("pos", 4);
		ads_cluster["ad_23"] = googletag.defineSlot("/4128/CBS.HARTFORD/" + cbs_ads_zone + "", [[1,6]], "ad_23").addService(googletag.pubads()).setTargeting("tag", cbs_ads_tags).setTargeting("pos", 23);
