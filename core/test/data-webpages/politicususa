    <!-- End Quantcast tag -->    <script type="text/javascript">
//<![CDATA[

	function getUrlVars() {
		var vars = [], hash;
		var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&amp;');
		for(var i = 0; i &lt; hashes.length; i++) {
			hash = hashes[i].split('=');
			vars.push(hash[0]);
			vars[hash[0]] = hash[1];
		}
		return vars;
	}

//]]>
    </script>
