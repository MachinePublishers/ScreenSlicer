	function log_support(rank,anchor_url)
	{
		var currentDate = new Date();
		var querystring = 'url='+encodeURIComponent(anchor_url)+'&rank='+rank+'&searchurl='+encodeURIComponent(window.location.href)+'&time='+encodeURIComponent(currentDate.getTime())+'&engine='+engine;
		$j.get('/cfusion/search/stats/userstats.cfm?'+querystring);
	}
