    <script src="/lib/com.adobe/lib/jquery-1.2.6.min.js">
    </script>
    <script type="text/javascript">
//<![CDATA[

var s_accountName = 'mxmacromedia';
var category = "all";
var engine='GSS';
var currentURLParams = "term%3Dibm%26loc%3Den%5Fus%26siteSection%3Dhome";
var $j = jQuery.noConflict();
$j(document).ready(function() {
  $j('#narrowSearch input').click(function(event) {
	var self = $j("#narrowSearch input[type='radio']:checked").val();
	currentURLParams = urldecode(currentURLParams).replace(/&?self=([^&]$|[^&]*)/i,'');
	currentURLParams = currentURLParams.replace(/&?site=([^&]$|[^&]*)/i,'');
	currentURLParams = currentURLParams.replace(/&?rf=([^&]$|[^&]*)/i,'');
	if (self == 2){
		currentURLParams = currentURLParams + '&self=1' + '&' + $j('#rf_hidden').val();
	} else {
		currentURLParams = currentURLParams + '&self='+self;
	}
	window.location.href = "index.cfm?" + currentURLParams;
});
});
function urldecode(url) {
  return decodeURIComponent(url.replace(/\+/g, ' '));
}

//]]>
    </script>
