  scripts: 
      
        
            
              
              
                ["https://static.licdn.com/scds/concat/common/js?v=build-2000_8_37789-prod&amp;f=lib%2Ftwitter-typeahead%2F0.9.3%2Ftypeahead.min"]
              
            
          
        
      
    
