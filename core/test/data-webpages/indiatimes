        <script type="text/javascript">
//<![CDATA[

var recordCount = 1;
var fetching = false;
function getMoreEst(searchParameter,pg,h){
		var holder = h;
		fetching = true;
		recordCount = pg;
		if(typeof(holder)!='undefined') {
			$(holder).html('Loading more &lt;span&gt;&lt;/span&gt;&amp;nbsp;').show();
		}
		
		$.get('http://www.indiatimes.com/contentsearchlazymore.php',{searchParameter:searchParameter,pg:pg},
		function(data){
			if(data != ""){				
				$('.loadmore').remove();
				//$('#showMoredata').append(data);
				document.getElementById('showMoredata').innerHTML += data;
				setTimeout(function(){
					fetching = false;
		      }, 500);
			}
		}
	);	
}
$(window).scroll(function(){
	var bufferZone = $(window).scrollTop() * 0.05;
	if (!fetching &amp;&amp; ($(window).scrollTop() + bufferZone &gt; ($(document).height()- $(window).height()))){
		if((recordCount % 3 == 0)){
			$('#moretext').html("More Stories&lt;span&gt;&lt;/span&gt;");
		}
		else{
			$('#moretext').click();
		}
	}
});

function getAjaxResponse(val){
	var val;
	$.get('http://www.indiatimes.com/contentsearchlazymore.php',{searchParameter: '&amp;q='+val,pg:1,ajaxResponse:1},function(data){
			document.getElementById('showMoredata').innerHTML = data;
	})
}

//]]>
        </script>
