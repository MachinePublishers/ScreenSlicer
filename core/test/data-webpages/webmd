            function addLoadEvent(func) 
		    { 
                var oldonload = window.onload;              
                if (typeof window.onload != 'function') 
                {   
                    window.onload = func;   
                } 
                else   
                {  
                    window.onload = function() 
                    {                  
                        oldonload();                  
                        func();                
                    }              
                }            
            }                    

            function setupFeatSection() 
            {       
                sliderHeight = $('#tapSlide').attr('scrollHeight')
                $('#tapSlide').css('display','none');
                $('#tapSlide').css('height',sliderHeight);
				$('.psAttrib h4').css('line-height', $('.tapLogo').height() + 'px');
                $('#TAPPremiumSearch1_tapPremium').hover(
                function() { hover_intent = setTimeout( function(){$('#tapSlide').show('slow');} , 250 ); },function() { try{clearTimeout(hover_intent);}catch(e){}; $('#tapSlide').hide('slow'); });     
            }                     
                 
            addLoadEvent(setupFeatSection);
            
			/* ]]> */ 
		
//]]>
