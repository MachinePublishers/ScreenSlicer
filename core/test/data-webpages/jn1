      $('.search_result .filter span').removeClass('selected');
      $(this).parent().addClass('selected');
      $('.search_result .type').attr('value', $(this).attr('rel'));
      $('.search_result .page').attr('value', 1);
      $('.search_result form').submit();
    });

    $('.search_result .page').live('click', function(){
      $('.search_result .page').attr('value', $(this).text());
      $('.search_result form').submit();
    });

    $('.search_result .navigate .next a').live('click', function(){
      $('.search_result .page').attr('value', parseInt($('.search_result .page').attr('value'))+1);
      $('.search_result form').submit();
    });

    $('.search_result .navigate .prev a').live('click', function(){
      $('.search_result .page').attr('value', parseInt($('.search_result .page').attr('value'))-1);
      $('.search_result form').submit();
    });

