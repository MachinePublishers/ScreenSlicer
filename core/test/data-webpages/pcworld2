		function renderBingAds(divId,ads) {
		   if (ads.length > 0) {
		       var adsBody = "<h2>Sponsored Links</h2>";
		       for (var i = 0; i < ads.length; i++) {
		           var ad = "<div class='ad '>";
		           ad += "<a href='" + ads[i].url + "' target='_blank' rel='nofollow' class='ad-title bingexit'>" + ads[i].title + "</a><br/>";
		           if (null != ads[i].line1 && ads[i].line1.length>0) {
		            ad += ads[i].line1 + "<br/>";
		           }
		           ad += "<a href='" + ads[i].url + "' class='ad-site bingexit' rel='nofollow' target='_blank'>" + ads[i].display_url + "</a>";
		           ad += "</div>";
		           adsBody += ad;
		           //if client is a mobile device, copy first add above search results
		           if ($thm.isMobile() && i==0){
		        	   $("#firstSponsoredResult").html("<h2>Sponsored Link</h2>" + ad);
