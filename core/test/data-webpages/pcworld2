			$('#banner-toggle a').click(function(event){
				openHeight = $('.int-features-inner').height() + 'px';
				wideToolHeight = 140 + $('.int-features-inner').height() + 'px';
				if($('#int-features').hasClass('open')){
					$('#int-features').animate({height:'0px'}).removeClass('open');
					$('#wide-panel-banner').animate({height:'140px'});
					$(this).removeClass('open');
				}else{
					$('#int-features').animate({height:openHeight}).addClass('open');
					$('#wide-panel-banner').animate({height:wideToolHeight});
					$(this).addClass('open');
				}
				return false;
				});
	

	
	$(document).ready(function(){

	$('#extra-nav-toggle a').click(function(){
		if($(this).hasClass('open')){
			$('#extra-nav').hide();
			$(this).removeClass('open');
			$('#extra-nav-toggle a').removeClass('ss-delete');
			$('#extra-nav-toggle a').addClass('ss-dropdown');		
