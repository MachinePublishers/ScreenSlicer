     window.fbAsyncInit = function() {
       FB.init({
         appId: "282931841805811",
         status: true,
         cookie: true,
         xfbml: true,
         oauth : true,
         channelUrl: "http:\/\/www.scmp.com\/fb_social\/channel"
         });
  
         FB.Event.subscribe("edge.create", function(href, widget) {
_gaq.push(["_trackEvent", "Facebook like", "Drupal", href]);
});
        var redirect_uri = 'http://www.scmp.com/fboauth/connect?destination=content%2Fsearch%2Fus';

        (function ($) {
          FB.Event.subscribe('auth.login', function(response) {
            var access_token = 'access_token=' + response.authResponse.accessToken;
            access_token = ((redirect_uri.indexOf('?destination=') &gt;= 0) ? '&amp;' : '?') + access_token;
            window.location = redirect_uri + access_token;
          });

          FB.Event.subscribe('auth.statusChange', function(response) {
            if (response.status == 'connected') {
              var access_token = 'access_token=' + response.authResponse.accessToken;
              access_token = ((redirect_uri.indexOf('?destination=') &gt;= 0) ? '&amp;' : '?') + access_token;

              // hide the true fb login div(s)
              $('div.fb-login-button').hide();

              // set the link on the 'fake' login button(s)
              if ($('div.scmp-fb-login-button a')) {
                $('div.scmp-fb-login-button a').each(function(i, e) {
                  e.href = redirect_uri + access_token;
                });
              }

              // make the 'fake' login button(s) visible
              $('div.scmp-fb-login-button').show();
            }
          });
        })(jQuery);
     };
     (function() {
       var e = document.createElement('script');
       e.async = true;
